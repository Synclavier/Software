OMSQueue_nitems			EQU	0OMSQueue_qhead			EQU	2OMSQueue_qtail			EQU	4OMSQueue_qsize			EQU	6OMSQueue_qSizeMinus1	EQU	8OMSQueue_data			EQU	10OMSSerialPort_sendDataProc      	EQU	   0OMSSerialPort_rcvByteProc       	EQU	   4OMSSerialPort_specialRcvProc    	EQU	   8OMSSerialPort_handshakeProc     	EQU	  12OMSSerialPort_nextPort          	EQU	  16OMSSerialPort_globalPortNum     	EQU	  20OMSSerialPort_portID            	EQU	  22OMSSerialPort_portInitialized   	EQU	  26OMSSerialPort_usingPort         	EQU	  27OMSSerialPort_portSpeed	        	EQU	  28OMSSerialPort_myTimeDBRA        	EQU	  30OMSSerialPort_vblProc           	EQU	  32OMSSerialPort_reserved2         	EQU	  36OMSSerialPort_hwDriverProc      	EQU	  40OMSSerialPort_xmtQ              	EQU	  44OMSSerialPort_xoffSendingSuspended	EQU	  48OMSSerialPort_serialErr         	EQU	  49OMSSerialPort_rcvQ              	EQU	  50OMSSerialPort_xoffReceivingSuspend	EQU	  54OMSSerialPort_packet            	EQU	  56OMSSerialPort_hwSCCRCtl         	EQU	  74OMSSerialPort_hwSCCWCtl         	EQU	  78OMSSerialPort_hwSCCWData        	EQU	  82OMSSerialPort_hwVector          	EQU	  86OMSSerialPort_hwSending         	EQU	 102OMSSerialPort_hwReceiving       	EQU	 103OMSSerialPort_needToSendHandshake	EQU	 104OMSSerialPort_handshakeByteToSend	EQU	 105OMSSerialPort_reserved          	EQU	 106	MACRO	AsmDeQ	&qp, &dest, &dx		move.w	OMSQueue_qhead(&qp),&dx		move.b	OMSQueue_data(&qp,&dx .w),&dest		subq.w	#1,OMSQueue_nitems(&qp)		addq.w	#1,&dx		and.w	OMSQueue_qsizeMinus1(&qp),&dx		move.w	&dx,OMSQueue_qhead(&qp)	ENDM		MACRO	LoadQTail	&qp, &dx		move.w	OMSQueue_qtail(&qp),&dx	ENDM		MACRO	StoreQTail	&qp, &dx		move.w	&dx,OMSQueue_qtail(&qp)	ENDM		MACRO	EnQ1		&qp, &src, &dx		move.b	&src, OMSQueue_data(&qp, &dx .w)		addq.w	#1, OMSQueue_nitems(&qp)		addq.w	#1,&dx		and.w	OMSQueue_qsizeMinus1(&qp),&dx	ENDM		MACRO	AsmEnQ		&qp, &src, &dx		LoadQTail	&qp, &dx		EnQ1		&qp, &src, &dx		StoreQTail	&qp, &dx	ENDM	MACRO	DISABLE_INTR	move.w	sr,-(sp)	ori.w	#$700,sr	ENDM		MACRO	ENABLE_INTR	move.w	(sp)+,sr	ENDM