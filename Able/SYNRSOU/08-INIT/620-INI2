/* 620-INI2 - $SUBTITLE - Initialization code for guitar *//*   5/15/86 - "official" creation of release-M modules*//* insert this code in guitar compilations.   *//* must insert before interrupts are enabled. */dcl restore.tuning proc external;dcl old.thr     fixed external;dcl onthr       fixed external;dcl g.split.loc fixed external;dcl pgholdsw    fixed external;if inc.axe then call restore.tuning;  /* go to normal tuning          */old.thr    =(-1);        /* set to this value to cause immediate scan */onthr      =200;         /* keep strings off until first scan */g.split.loc=2;           /* define initial split string  */pgholdsw   =1;           /* assume hold switch held - release is less obnoxious than pressing!! */if fake.it<>0 then do;   /* initialize pseudo data for testing with no guitar */   fake.datas(0)=255;    /* set max volume */   fake.datas(1)=255;    /* and also max rte */   fake.datas(3)=255;    /* assume max vol pedal */   fake.datas(4)=255;    /* also max rte pedal */   fake.datas(5)=255;    /* and hold switch off */end;