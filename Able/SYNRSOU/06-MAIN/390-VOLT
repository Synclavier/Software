/* $PAGE - CHECK FOR NEW FILTER CONTROL VOLTAGE SETTINGS   Modified:   02/07/91 - MWH - Combine 3200, 6400 and 9600 RTP's   05/15/86 - "official" creation of release-M modules*/  /* EMIT GATE/TRIGGER                            */  /* PROCESS FILTER SETTINGS & CHECK FOR TRACKING */BEGIN;   IF D164_PRESENT THEN DO;      IF RESET.FILTERS<>0 THEN DO;    /* WRITE OUT FILTER DATA */        J=LP.CUT;                     /* LOW PASS INFO         */        IF (ANY.TRACKING&BIT.LPT)<>0 THEN J=J+TRACK.DATA;        IF J>250 THEN J=250;        DISABLE;        WRITE(D164)=0; WRITE(D165)=J; /* WRITE OUT LP FILTER CV */        ENABLE;        J=HP.CUT;                     /* HIGH PASS INFO         */        IF (ANY.TRACKING&BIT.HPT)<>0 THEN J=J+TRACK.DATA;        IF J<0 THEN J=0; IF J>250 THEN J=250;        DISABLE;        WRITE(D164)=1; WRITE(D165)=J; /* WRITE OUT HP CV */        ENABLE;        J=BP.CUT;                     /* BP INFO */        IF (ANY.TRACKING&BIT.LPT\BIT.BPT)<>0 THEN J=J+TRACK.DATA;        IF J<0 THEN J=0; IF J>250 THEN J=250;        DISABLE;        WRITE(D164)=2; WRITE(D165)=J; /* WRITE OUT BP CV */        WRITE(D164)=3; WRITE(D165)=BP.WID XOR 128; /* WRITE OUT Q CV */        ENABLE;        RESET.FILTERS=0;      END;      /* OF WRITE OUT FILTER DATA */   END;      /* OF D164_PRESENT */END;      /* OF BEGIN FOR THIS FILE */