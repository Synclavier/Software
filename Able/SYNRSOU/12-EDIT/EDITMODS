/* editmods  $TITLE  Insert file for sequence editing functions   Modified:   01/30/91 - MWH - Report which tracks have had new notes   07/21/87 - MWH - Update all to support max 4 poly bins, 512mb/bin   12/01/86 - ts - added insert of file CHAIN   09/26/86 - eeg - created this file*/module editmods;   insert ':synlits:allslits';   insert ':-xpl:catrout';     /* get catalog routines */   insert ':-xpl:intrprtr';   INSERT ':SYNRDCLS:03-PDCLS';   INSERT ':SYNRDCLS:04-SDCLS';   INSERT ':SYNRDCLS:05-BDCLS';   INSERT ':SYNRDCLS:11-TDCLS';   insert ':synmods:allrmods';   insert ':synmods:seqdcl';           /* for EXTRACT.KEY & EXTRACT.DUR */   enter ':SYNRSOU:12-edit';           /* get source files    */      insert 'erase';                  /* routine to erase a track */      insert 'editprim';               /* primitive functions (insert & delete) */      insert 'bounce';                 /* routine to bounce tracks */      insert 'chain';                  /* contains routine to do chaining */      insert 'diskio';                 /* buffered disk output routines */      insert 'editfile';               /* ClipBoard Management Code */      insert 'cut';                    /* routines for cut/copy/move function */      insert 'paste';                  /* routines for paste/merge/fill function */end editmods;