/* MOUSELIT   $TITLE  LITERALS FOR MOUSE CODE   Modified:   05/18/88 - LSS - added system config item MOUSE_PRESENT   04/22/88 - TSS,KJO - added termulator literal   11/25/87 - MWH - Remove unused "d40q_in_system"   */DCL MOUSE.PORT.IS.D50            FIXED EXTERNAL;  /* 1 => D50 (MAC II), 0 => D44 (LOGITECH) */DCL TERMULATOR  LIT '(mouse.port.is.d50)';DCL D44_PRESENT LIT '(INC.MOUSE and MOUSE_PRESENT)';DCL M$HALT             LIT '0';DCL M$POSITION         LIT '1';DCL M$STATUS           LIT '2';DCL M$DIAGNOSTIC       LIT '3';dcl button.check       lit '0';dcl current.x          lit '1';dcl current.y          lit '2';dcl press.x            lit '3';dcl press.y            lit '4';dcl release.x          lit '5';dcl release.y          lit '6';dcl button.result      lit '7';dcl delta.x            lit '8';dcl delta.y            lit '9';dcl press.time         lit '10';dcl release.time       lit '11';DCL MOUSE_COURSE_STYLE lit '0';DCL MOUSE_FINE_STYLE   lit '1';