/* MATHLITS   $TITLE  Literal definitions for MATHMOD   Modified:   10/24/88 - MWH - Remove unused (also wrong!) "footage mode" literals   06/15/87 - MWH - Add 32-bit math literals   */dcl sm#drop       lit '0';  /* SMPTE MODE: 30 fps     drop frame mode */dcl sm#nondrop    lit '1';  /* SMPTE MODE: 30 fps non-drop frame mode */dcl sm#25fps      lit '2';  /* SMPTE MODE: 25 fps                mode */dcl sm#24fps      lit '3';  /* SMPTE MODE: 24 fps                mode */dcl cf#time       lit '0';  /* indicates number being converted is a time */dcl cf#dur        lit '1';  /* indicates number being converted is a duration */dcl BitMsbArray   lit 'loc(addr(BITMSB))';