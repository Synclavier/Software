/* TTYLITS $Title Terminal I/O specific literals *//* MODIFIED:   09/14/88 - TSS - Added lits for protocol message handler*/dcl tty.buf.len       lit '128';  /* 128 char output buf length */dcl protocol.in.len   lit '2048'; /* MUST BE A POWER OF 2 */dcl protocol.out.len  lit '512';  /* MUST BE A POWER OF 2 */dcl screen.image.len  lit '960';  /* length of screen dump in words. (24*80=1920 bytes or 960 words) */dcl norm              lit '0';    /* code for normal character mode    (stored in ch.mode) */dcl alt               lit '1';    /* code for alt    character mode    (stored in ch.mode) *//* For calls to Set.Term.Attr */dcl ATTR#Normal       lit '128';dcl ATTR#Reverse      lit '1\256';dcl ATTR#NoReverse    lit '1';dcl ATTR#AltIntensity lit '2\512';dcl ATTR#RegIntensity lit '2';dcl ATTR#Blink        lit '4\1024';dcl ATTR#NoBlink      lit '4';dcl ATTR#Underline    lit '8\2048';dcl ATTR#NoUnderline  lit '8';