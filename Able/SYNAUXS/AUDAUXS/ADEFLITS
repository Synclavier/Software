/*	:SYNAUXS:AUDAUXS:ADEFLITS	literals for default settings file	*//*Modified:1999/03/04 - TY  - Added miscellaneous defaults1989/03/19 - TSS - Added MIDI Input Chan and Echo mode defaults1989/01/05 - SGS - Added SMPTE display offset default1988/11/16 - TSS - Adjusted sector offsets to support .SDEF-7 file1988/07/26 - SGS - Created.*//*	Sector offsets into DEFSFILE	*/dcl Info#AEEPanelMacros	lit '  0';	/*	Panel layout macros	*/dcl Info#AEEDefaults		lit '  1';	/*	AEE default settings	*/dcl Info#Sectors.Used	lit '  2';	/*	number of sectors used in .SDEF-7 file	*//*	FIRST SECTOR OF DEFAULTS:	*/		//	APPARENTLY THIS IS AT SECTOR 1 (NOT 0.)  -TYdcl AEE#DefsInit			lit '  0';	/*	0=defaults uninitialized; 1=initialized	*//*	Project Manager Panel:	*/dcl AEE#InRout				lit '  1';	/*	Input routings (up to 16 tracks)	*/dcl AEE#OutRout			lit ' 17';	/*	Outpt routings (up to 16 tracks)	*/dcl AEE#SampRate			lit ' 33';	/*	DTD sample rate						*//*	Free: 34-49	*//*	Synchronization Panel:	*/dcl AEE#SMPTEIn			lit ' 50';	/*	SMPTE in type and rate				*/dcl AEE#SMPTEDisp			lit ' 51';	/*	SMPTE display type and rate		*/dcl AEE#SMPTEOffs			lit ' 52';	/*	SMPTE offset:	 Hrs					*//*										53								 Min					*//*										54								 Sec					*//*										55								 Fra					*//*										56								 Bits					*/dcl AEE#FootDisp			lit ' 57';	/*	Footage display type and rate		*/dcl AEE#FootOffs			lit ' 58';	/*	Footage offset: Feet					*//*										59								 Frames				*//*										60								 Fraction			*/dcl AEE#DispFormat		lit ' 61';	/*	Time display format					*/dcl AEE#SMPTEDspOffs		lit ' 62';	/*	SMPTE display offset:				*//*										63			Stored as SBits (32-bit number)	*//*	Free: 64-69	*//*	Record Panel:			*/dcl AEE#RecordMode		lit ' 70';	/*	Record mode:	Mode			*//*										71								Trigger		*//*										72								Rec			*//*	MIDI Page Defaults:	*/dcl MIDI#InputChan		lit ' 73';	/*	Default MIDI Input Channel	*/dcl MIDI#EchoDisable		lit ' 74';	/*	Default MIDI Echo State		*///	Miscellaneous Defaults:dcl Misc#DefsInit			lit ' 75';	//	0 = defaults uninitialized, 1 = initializeddcl Misc#KbdRouting		lit ' 76';	//	Default Keyboard Multichannel Routing	(for initialization)dcl Misc#ClickOn			lit ' 77';	//	Default On/Off status of the click		(for initialization)dcl Misc#ClickDispMode	lit ' 78';	//	Default Click Display Mode					(for initialization)dcl Misc#ClickRate		lit ' 79';	//	Default Click Rate							(for null sequence)dcl Misc#Speed				lit ' 80';	//	Default Speed									(for null sequence)dcl Misc#ActiveMIDIRTEs	lit ' 81';	//	Default MIDI RTEs								(for null timbre)dcl Misc#VelocitySens	lit ' 82';	//	Velocity Sensitivity							(for null timbre)dcl Misc#FinalDecay		lit ' 83';	//	Default Final Decay							(for loading sound files)/*	Free: 84-255	*//*	SECOND SECTOR OF DEFAULTS:	*/		//	APPARENTLY THIS IS AT SECTOR 0.  -TY/*	Panel Menu:						*/dcl AEE#PanLayout			lit '  0';	/*	Panel Layout Memories	*//*	Looks like 4 12-word records are stored	*//*	8 words describing layout						*//*	4 words with 6-char xpl string name...		*//*	Free:  48-127	*//*	at words 128:					*//*	4 32-word records				*//*	one for each panel macro	*/dcl D#MacroPanels			lit '  0';	/*	offset in structure of panel list			*/dcl D#MacroName			lit '  8';	/*	offset in structure of macro name			*/dcl D#MacroGlobals		lit ' 12';	/*	offset in structure of globals				*/dcl D#MacroSize			lit ' 44';	/*	size of structure									*/dcl D#MacroSeqTracks		lit ' 12';	/*	in globals ==> Seq.Tracks						*/dcl D#MacroSeqContents	lit ' 13';	/*	in globals ==> Display.Note.Contents		*/dcl D#MacroSeqLength		lit ' 14';	/*	in globals ==> Display.Name.Length			*/dcl D#MacroPanelBase		lit '  0';	/*	on disk ==> offset in sector of panels		*/dcl D#MacroPanelStride	lit ' 12';	/*	on disk ==> stride of panels/name			*/dcl D#MacroGlobalBase	lit '128';	/*	on disk ==> offset in sector of globals	*/dcl D#MacroGlobalStride	lit ' 32';	/*	stride & size of globals on disk...			*//*	Bits used determine which defaults to read/write	*/dcl D#In				lit '        1';	/*	input routings				*/dcl D#Out			lit 'shl(1, 1)';	/*	output routings			*/dcl D#Samp			lit 'shl(1, 2)';	/*	DTD sample rate			*/dcl D#SIn			lit 'shl(1, 3)';	/*	SMPTE In Type				*/dcl D#SDisp			lit 'shl(1, 4)';	/*	SMPTE disp Type			*/dcl D#SOffs			lit 'shl(1, 5)';	/*	SMPTE Offset				*/dcl D#FDisp			lit 'shl(1, 6)';	/*	Footage disp Type			*/dcl D#FOffs			lit 'shl(1, 7)';	/*	Footage Offset				*/dcl D#RecM			lit 'shl(1, 8)';	/*	Record Mode					*/dcl D#DForm			lit 'shl(1, 9)';	/*	Time Disp Format			*/dcl D#PanLay		lit 'shl(1,10)';	/*	Panel Layout Mem			*/dcl D#SDispOffs	lit 'shl(1,11)';	/*	SMPTE Display Offset		*/dcl D#MIDI			lit 'shl(1,12)';	/*	MIDI Defaults				*/dcl D#Misc			lit 'shl(1,13)';	/*	Miscellaneous Defaults	*/dcl D#RES14			lit 'shl(1,14)';	/*	Unused....					*/dcl D#RES15			lit 'shl(1,15)';	/*	Unused....					*/