/* STMLITS - LITERALS FOR STM SCREEN *//*   08/02/89 - SJS - Added SRC literal   09/08/88 - SJS - Changed TSM length to SFTime format   08/25/88 - MWH - Add "real" DSP user interface   06/06/88 - SJS - Add DSP literal   04/22/88 - JSS - added/changed max & min gain vals for new/old SAFE (ADC)   04/15/88 - MWH - Add point mode and zero line   08/19/87 - MWH - add max time command to store mode   10/21/86 - BSW - added a literal for mark offset    9/19/86 - BSW - added some more literals    8/15/86 - eeg - created this file*/dcl origname lit '''ORIGIN''';    /* ORIGIN symbol */dcl EDITBUFF lit '''.EDITBUF''';  /* edit buffer name */dcl markoff  lit '''OFFSET''';    /* mark OFFSET symbol */dcl MIXLEVEL lit '''1''';         /* default volume level for mix *//* captions are all an even number of characters long */dcl loopcap  lit '''Looping Sound File''';dcl extcap   lit '''Extracted Data''';dcl revcap   lit '''Reversed Data ''';dcl samcap   lit '''Sampled Data''';dcl mixcap   lit '''Mixed Data''';dcl modcap   lit '''Modulated Data''';                 dcl total.time            lit '25';  /* scale of window used for drawing time tic marks */dcl command.row           lit '18';  /* location of first row of commands */dcl first.command.column  lit '1';   /* locations of columns of commands */dcl second.command.column lit '18';dcl third.command.column  lit '29';dcl fourth.command.column lit '43';dcl fifth.command.column  lit '56';dcl sixth.command.column  lit '69';dcl stm.display           lit '0';  /* number representing display command */dcl stm.modify            lit '1';  /* represents modify commands */dcl stm.store             lit '2';  /* represents store/recall commands */dcl stm.modify2           lit '3';  /* represents modify II commands*//* codes for Display STM commands */dcl stm.show              lit '3';dcl stm.zoom              lit '4';  /* number representing zoom command */dcl stm.search            lit '5';dcl stm.label             lit '6';  /* number representing label command */dcl stm.unlabel           lit '7';  /* number representing unlabel command */dcl stm.center            lit '8';dcl stm.mark.start        lit '9';  /* number representing mark start command */dcl stm.mark.end          lit '10'; /* number representing mark end command */dcl stm.left              lit '11';dcl stm.symbols           lit '12';dcl stm.print             lit '13';dcl stm.help              lit '14';dcl stm.cap               lit '15';dcl stm.mark.off          lit '16';dcl stm.lock              lit '17';dcl stm.unlock            lit '18';dcl stm.dot.mode          lit '19';dcl stm.zero              lit '20';/* codes for Modify STM commands */dcl stm.reverse           lit '3';  /* number representing reverse command */dcl stm.cut               lit '4';dcl stm.paste             lit '5';dcl stm.extract           lit '6';dcl stm.delete            lit '7';dcl stm.exchange          lit '8';dcl stm.combine           lit '9';dcl stm.extmono           lit '10';dcl stm.cross             lit '11';dcl stm.copy              lit '12';dcl stm.fill              lit '13';dcl stm.loop              lit '14';dcl stm.volume            lit '15';dcl stm.normalize         lit '16';dcl stm.modulate          lit '17';dcl stm.revloop           lit '18';dcl stm.mix               lit '19';dcl stm.undo              lit '20';dcl stm.invert            lit '21';dcl stm.dctrim            lit '22';/* codes for store/recall STM commands */dcl stm.save              lit '3';dcl stm.unsave            lit '4';dcl stm.rename            lit '5'; /* number representing rename command */dcl stm.recall            lit '6'; /* number representing recall command */dcl stm.collect           lit '7';dcl stm.record            lit '8'; /* number representing record command */dcl stm.max.time          lit '9';/* codes for MODIFY 2 STM commands */dcl stm.tsm               lit  '3';dcl stm.src               lit  '4';dcl stm.monoize           lit  '7';dcl stm.debug             lit '22';dcl stm.max.vert.factors  lit '17';  /* maximum number of scale factors in zoom tables */dcl stm.max.hor.factors   lit '25';  /* maximum number of scale factors in zoom tables */dcl pixel.samples         lit  '127'; /* largest AD signal divided by largest vertical scale factor plus 1: 65535/512=127 */dcl display.origin        lit  '64';  /* mapped pixel location of origin within the signal display 1/2 of pixels.samples */dcl stm.old.gain.min      lit  '10';  /* recording scale min linear val*10 for old SAFE (Analog Solutions ADC) */dcl stm.old.gain.max      lit  '810'; /* recording scale max linear val*10 for old SAFE */dcl stm.new.gain.min      lit  '-3';  /* recording scale min dB val for new SAFE (Analogic ADC) */dcl stm.new.gain.max      lit  '28';  /* recording scale max dB val for new SAFE */dcl row.offset   lit '7';     /* where list of symbols start in the symbol catalog */dcl sym.name.col lit '2';     /* offset from the left margin for symbol name in symbol catalog */dcl sym.time.col lit '13';    /* offset from the left margin where symbol time starts */// Device selections for stm.record.devicedcl stm.record.device.d66		lit '0';			// use D66dcl stm.record.device.stm		lit '1';			// use STMdcl stm.record.device.dig		lit '2';			// use Digital STM