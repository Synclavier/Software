hronization variable          */	dcl midi.formaty.prompt   lit ' 7';	 /* y location of midi output sync type prompt & variable       */	dcl midi.formatx.prompt   lit '58';  /* x location of midi output sync type prompt                  */	dcl midi.formatx          lit '70';  /* x location of midi output sync type variable                */   	dcl midi.echoy.prompt     lit ' 8';  /* y location of midi echo prompt & variable                   */   dcl midi.echox.prompt     lit '41';  /* x location of midi echo prompt                              */   dcl midi.echox            lit '51';  /* x location of midi echo variable                            */	dcl oms.onoffy.prompt     lit ' 8';  /* y location of midi echo prompt & variable                   */   dcl oms.onoffx.prompt     lit '59';  /* x location of midi echo prompt                              */   dcl oms.onoffx            lit '70';  /* x location of midi echo variable                            */