/* TPRIMLIT - SPECIAL LITS FOR TPRIM MODULE */dcl ccatpos       lit '46';   /* normal location of current catalog on screen */dcl polymem.dev   lit '08';    /* use this number to refer to poly memory */dcl sndcache.dev  lit '09';    /* use this number to refer to polymem sound file cache *//* literals for types of input processing */dcl in.chin            lit '00';    /* character input           */dcl in.abss            lit '01';    /* absolute select           */dcl in.scdn            lit '02';    /* scroll down               */dcl in.scup            lit '03';    /* scroll up                 */dcl in.rtps            lit '04';    /* return to previous screen */