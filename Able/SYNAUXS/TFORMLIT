   /* Literals used to control FORMATTING of number conversions */   dcl cf#unsigned   lit '00'; /* CONVERSION FORMAT bit ==> unsigned value */   dcl cf#signed     lit '01'; /* CONVERSION FORMAT bit ==>   signed value */   dcl cf#erase      lit '00'; /* CONVERSION FORMAT bit ==> erase string before conversion */   dcl cf#append     lit '02'; /* CONVERSION FORMAT bit ==> append new chars to string */   dcl cf#left       lit '00'; /* CONVERSION FORMAT bit ==> left  justify output */   dcl cf#right      lit '04'; /* CONVERSION FORMAT bit ==> right justify output */   dcl cf#nofill     lit '00'; /* CONVERSION FORMAT bit ==> no filling of output field */   dcl cf#spfill     lit '08'; /* CONVERSION FORMAT bit ==> fill field with spaces */   dcl cf#zerofill   lit '16'; /* CONVERSION FORMAT bit ==> fill field with zeroes */   dcl cf#forcedp    lit '32'; /* CONVERSION FORMAT bit ==> force decimal point at end of string */   dcl cf#subspace   lit '64'; /* CONVERSION FORMAT bit ==> substitute single space for '0'      */   dcl cf#forcemin   lit'128'; /* CONVERSION FORMAT bit ==> force minus sign before digits       */   dcl cf#signmask   lit '01'; /* CONVERSION FORMAT mask ==> access sign format */   dcl cf#appendmask lit '02'; /* CONVERSION FORMAT mask ==> access append/erase info */   dcl cf#justmask   lit '04'; /* CONVERSION FORMAT mask ==> access justification info */   dcl cf#fillmask   lit '24'; /* CONVERSION FORMAT mask ==> access fill style info */   dcl cf#suppress   lit '-1'; /* used for FW$RIGHT to suppress field */   dcl cf#default    lit ' 0'; /* used for FW$LEFT and FW$RIGHT to get default field widths */