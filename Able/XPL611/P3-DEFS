/* Pass3 definitions: */  dcl divmax      lit '256';  /* maximum number of ram divisions */dcl stacklen    lit '512';  /* stack size */dcl istacklen   lit '(2*ins_levels)'; /* insert file stack length */dcl pstacklen   lit 's.depth.max'; /* procedure stack length */dcl insmax      lit '128';  /* allow 128 x 16 = 2048 insert statements/program */dcl ifbufl_min  lit '512';  /* minimum input file buffer length (MUST be >= 512) */dcl ofbufl      lit '2048'; /* output file buffer length (MUST be >= 512) */dcl ovbufl      lit '4096'; /* overlay file buffer length */dcl sfbufl      lit '512';  /* size of symbol table output buffer */dcl cutoff      lit '3';    /* maximum number of registers saved in-line on proc entry */dcl rtp_diff    lit '400';  /* approximate difference between lengths of RTPs (len(RTC) - len(RTB)) */dcl key_ratio   lit '3/5';  /* fraction of memory reserved for KEYs */dcl t.executable  lit '1';  /* executable file file type */dcl t.relocatable lit '2';  /* relocatable binary file type *//* linker definitions */dcl lib_size      lit '200'; /* maximum number of libraries linker can handle */dcl lib_name_size lit '(lib_size*10)'; /* maximum area for library names */dcl hash_size     lit '511'; /* size of linker hash table (must be 2^n - 1) */dcl total_symbols lit '400'; /* maximum number of symbols linker can handle (internal memory) */dcl most_symbols  lit '6553'; /* can never handle more symbols than these (just 65536/SYMBOL_LENGTH) */dcl symbol_length lit '10'; /* average symbol length = 18 chars; can be decreased 1 char by allocation rounding */dcl stentry_size  lit '(symbol_length + 10)'; /* number of words in a symbol table entry */dcl stable_round  lit '(stentry_size*3/10)'; /* round up .7 and up */dcl name_size     lit '64'; /* maximum size of linker name block */dcl devtablen     lit '14'; /* length of device driver table */dcl devdrivers    lit '10'; /* number of words of that table that are device driver pointers (rest are for FIND_DEVICE) */dcl spectablen    lit ' 1'; /* special labels - used by pass 3 only,  not rtp *//* runtime package routines */dcl sreg lit 'rfbuf (l.sreg)'; /* save registers routine    */dcl lreg lit 'rfbuf (l.lreg)'; /* load registers            */dcl retn lit 'rfbuf (l.retn)'; /* return routine            */dcl swpe lit 'rfbuf (l.swe )'; /* model c,d swap proc entry *//* communication area defs */dcl l.icell lit 'icell'; /* interrupt cells for when */dcl l.ctab  lit 'ctab'; /* configuration table */dcl ram      lit 'r (l.ram)';dcl pdll     lit 'r (l.pdll)';dcl max_refs lit 'r (l.extloc)'; /* maximum number of external references the linker can handle/library */dcl main_key lit 'r (l.main.key)'; /* key of main program (when linking) */  dcl prctyp lit '(shr(r (l.ctab + 1), 8) and 7)'; /* configuration area defs */dcl muldiv lit '((r (l.ctab + 1) and "10") <> 0)';dcl redrct lit 'r (l.ctab + 4)';dcl proc_type lit '(shr(core(loc.ctab + 1), 8) and 7)'; /* highcore config */dcl outdev    lit 'core(loc.ctab + 4)'; /* highcore config *//* the following is used to check to see if the swapcode bit is set dcl swapcode_set lit 'not(shr((keyflag(swap_key + proc_flags) and p.swapcode),2))';*/