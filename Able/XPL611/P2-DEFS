/* Pass2 definitions: */dcl sfbufl  lit '2048';  /* source file buffer (must match pass1 output buffer (IFBUFL) and .WORK) */dcl ifbufl  lit '2048';  /* output file buffer */dcl min.ptr.reg lit '"4"'; /* lowest  available pointer register */dcl max.ptr.reg lit '"11"'; /* highest available pointer register */dcl #blocks   lit  '300'; /* temp blocks */dcl stacksize lit '2000'; /* push down stack */dcl fstklen   lit   '20'; /* stack for floating items *//* configuration area definitions */dcl prctyp lit '(shr(r (l.ctab + 1), 8) and 7)'; /* configuration area defs */dcl muldiv lit '((r (l.ctab + 1) and "10") <> 0)';