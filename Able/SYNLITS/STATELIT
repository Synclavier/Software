/*	:SYNLITS:STATELIT  $Title  Input State LiteralsMODIFIED:1999/10/10 - TY  - Added s.conductor state literal1989/06/05 - TSS - Added S.KBD.MAP state literal*/dcl state.stack.len lit  '3';dcl num.states		lit '42';	/*	total number of input states	*//* NOTE: INPUT.STATE can have the following values: */dcl s.null           lit '00'; /* state 00 - NOP State           */dcl s.welcome        lit '01'; /* state 01 - Welcome Menu Screen */dcl s.main           lit '02'; /* state 02 - Main    Menu Screen */dcl s.timbre.dir     lit '03'; /* state 03 - Timbre     Directory Screen */dcl s.recall.snd     lit '04'; /* state 04 - Sound File Directory Screen  */dcl s.recall.seq     lit '05'; /* state 05 - Sequence   Directory Screen */dcl s.enter          lit '06'; /* state 06 - Subcatalog Directory Screen */dcl s.system         lit '07'; /* state 07 - System Command       Screen */dcl s.tds.acomp      lit '08'; /* state 08 - TDS Alpha    Comparative Display */dcl s.tds.gcomp      lit '09'; /* state 09 - TDS Graphics Comparative Display */dcl s.tds.part       lit '10'; /* state 10 - TDS Partial Timbre Display   */dcl s.tds.gfram      lit '11'; /* state 11 - TDS Graphic Harmonic Display */dcl s.tds.afram      lit '12'; /* state 12 - TDS Alpha   Harmonic Display */dcl s.tds.hard       lit '13'; /* state 13 - TDS Hardcopy Function        */dcl s.record         lit '14'; /* state 14 - Memory Recorder Display */dcl s.multi.out      lit '15'; /* state 15 - Multi Channel Output Screen */dcl s.assign.synth   lit '16'; /* state 16 - Assignment Menu for Synth Partial */dcl s.assign.patch   lit '17'; /* state 17 - Assignment Menu for Patch List Partial */dcl s.assign.instr   lit '18'; /* state 18 - Assignment Menu Instructions */dcl s.patch          lit '19'; /* state 19 - Keyboard Patch Display */dcl s.timbre.name    lit '20'; /* state 20 - Timbre Name Mode */dcl s.library        lit '21'; /* state 21 - Timbre Library Screen */dcl s.midi           lit '22'; /* state 22 - Midi Routing Display */dcl s.seqed          lit '23'; /* state 23 - Sequence Editor Screen */dcl s.stm.editor     lit '24'; /* state 24 - Sample-to-Memory Sound Editor Screen */dcl s.debug          lit '25'; /* state 25 - Debugging Screen */dcl s.song.directory lit '26'; /* state 26 - Song Directory Screen */dcl s.track.display  lit '27'; /* state 27 - Direct-to-Disk Track Display */dcl s.multi.tab      lit '28'; /* state 28 - multi screen with no redraw */dcl s.midi.tab       lit '29'; /* state 29 - midi  screen with no redraw */dcl s.mr.editor      lit '30'; /* state 30 - memory recorder editor */dcl s.audio.event    lit '31'; /* state 31 - audio event editor     */dcl s.optical        lit '32'; /* state 32 - optical disk           */dcl s.pf1            lit '33'; /* pf1                               */dcl s.pf2            lit '34'; /* pf2                               */dcl s.pf3            lit '35'; /* pf3                               */dcl s.pf4            lit '36'; /* pf4                               */dcl s.break          lit '37'; /* break                             */dcl s.tds.main       lit '38'; /* state 38 - meta state for f/tds         */dcl s.wait           lit '39'; /* state 39 - wait for <RETURN>            */dcl s.kbd.map        lit '40'; /* state 40 - keyboard mapping screen */dcl s.conductor		 lit '41'; /* state 41 - conductor screen			*/