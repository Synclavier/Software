/* ALLSLITS - ALL SYNCLAVIER LITERALS FOR THE REAL TIME CODE *//* FOR CONVENIENCE, INSERT ALL LITERAL FILES REQUIRED BY     *//* THE REAL TIME CODE                                        */insert ':synlits:COMLITS';           /* get configuration and pdl size */insert ':synlits:ASCIILIT';insert ':synlits:FCODLITS';insert ':synlits:GLOBLITS';insert ':synlits:LPNLITS';insert ':synlits:PRMLITS';insert ':synlits:QUELITS';insert ':synlits:SAMPLITS';insert ':synlits:SYSLITS';insert ':synlits:TIMLITS';insert ':synlits:THDLITS';insert ':synlits:SYNCLITS';DCL BETA.TEST       LIT '1';dcl debug           lit '1';dcl newcat          lit '1';dcl poly.splice     lit '0';     /* indicate poly splicing code with flag */dcl inc.nokbd       lit '1';     /* set to 1 to handle no keyboard */DCL TIM.DEBUG       LIT '0';/* guitar simulation flags     */dcl fake.it         lit '0';     /* set flag to 0 to delete pseudo guitar code */dcl fake.freq       lit  'core'; /* must declare these for compilation, but references are deleted */dcl fake.vol        lit  'core';dcl fake.datas      lit  'core';dcl fake.lites      lit  'core(0)';