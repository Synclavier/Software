/* GLOBLITS  $TITLE  File of General Literals Used by Most Programs *//*Modified:2007/02/02 - CJ  - Change NUM.MIDI.TRACKS to MAX.INPUTS = 2021998/05/15 - TY  - Corrected a misleading comment for LAST.LOD.TRACK.1998/05/09 - TY  - Added "is.negative" and "is.odd".1990/03/09 - PF  - Change NUM.MIDI.TRACKS to MAX.INPUTS = 1281989/03/14 - PF  - Added DEFAULT.MIDI.VELOCITY1988/06/22 - EEG - Moved various track literals (tracks above 200) here1988/05/04 - TSS - MOVED MIDI.MIDDLE.C HERE FROM 142-MIDI1987/06/15 - MWH - Added 32 bit address literals1987/01/20 - TS  - ADDED POLYPHONY.DISP LITERAL1986/11/17 - EG  - CHANGED SWAPABLE TO RECURSIVE SWAP1986/11/04 - BSW - increased the size of the character input buffer*//* XPL literals: */DCL SWAPABLE LIT 'RECURSIVE SWAP'; DCL FORWARD  LIT 'PROCEDURE';DCL FOREVER  LIT 'WHILE 1'  ;DCL TRUE     LIT '1'        ;DCL FALSE    LIT '0'        ;DCL BOOLEAN  LIT 'FIXED'    ;DCL ATNV     LIT '"324"'    ; 	 /* LIT FOR ABS TRANSFER NEVER *//* MULTIPLY DIVIDE LITERALS: */DCL LOAD  LIT 'WRITE(5)=';        /* LOAD               */DCL ULOAD LIT 'WRITE(4)=';        /* LOAD UPPER         */DCL REM   LIT 'READ(4)'  ;        /* QUOTIENT REMAINDER */DCL MUL   LIT 'WRITE(6)=';        /* PERFORM MULTIPLY   */DCL DIV   LIT 'WRITE(7)=';        /* PERFORM DIVIDE     */DCL RES   LIT 'READ(5)'  ;        /* RESULT LSB         */DCL URES  LIT 'READ(4)'  ;        /* RESULT MSB         */DCL MWAIT LIT 'WRITE("324")=RES'; /* D5 TO ATNV TO WAIT FOR MUL/DIV *//* REPEAT COUNTER (MODEL C PROCESSOR ONLY): */  DCL RPC   LIT 'WRITE("010")=(-1)+';/* EXTERNAL MEMORY DEFINITIONS */dcl mam    lit '"60"';     /* memory address msb */dcl mal    lit '"61"';     /* memory address lsb */dcl md     lit '"62"';     /* memory data */dcl mdi    lit '"63"';     /* memory data with increment *//* SERIAL PORT DEVICE ADDRESSES */DCL D50 LIT '"50"'; DCL D51 LIT '"51"'; /* TERMINAL PORT */DCL D40 LIT '"40"'; DCL D41 LIT '"41"'; /* PRINTER  PORT */DCL D42 LIT '"42"'; DCL D43 LIT '"43"'; /* VOICE    PORT */DCL D44 LIT '"44"'; DCL D45 LIT '"45"'; /* MOUSE    PORT */DCL D54 LIT '"54"';                     /* BITS FOR D4X INTERRUPT STATUS *//* THE SMPTE BOARD IS ACCESSED USING 4 DEVICE ADDRESSES: */DCL SMPTE.SUBSYS   LIT '8';    /* SUBSYSTEM # FOR SMPTE CARD */DCL D70            LIT '"70"'; /* WRITE 8 TO RESET  */DCL D71            LIT '"71"'; /* STATUS   BITS     */  /* WRITE OPCODES HERE */DCL D72            LIT '"72"'; /* SECONDS  FRAMES   */DCL D73            LIT '"73"'; /* HOURS    MINUTES  */  /* READ OF D73 TRIGGERS NEW INFO *//* FOR MIDI BOARD - NOTE: THEY MATCH SMPTE */DCL SYSSEL         LIT '"70"';  /* MIDI SUBSYSTEM SELECT ADDRESS   */DCL MCHA           LIT '"71"';  /* CHANNEL SELECT ADDRESS IF WRITE */DCL MSTA           LIT '"71"';  /* INPUT FIFO STATUS      IF READ  */DCL MDAT           LIT '"72"';  /* INPUT AND OUTPUT DATA PORT      *//* SCIENTIFIC TIMER DEVICE ADDRESSES */DCL D16            LIT '"16"';DCL D17            LIT '"17"';/* MONO SAMPLING SYNTH ADDRESSES */DCL D66    LIT '"66"'; /* CONTROL REGISTER */DCL D67    LIT '"67"'; /* DATA    REGISTER *//* GUITAR DEVICE ADDRESSES */dcl D134    lit '"134"';dcl D135    lit '"135"'; /* MULTI-CHANNEL DISTRIBUTOR DEVICE ADDRESSES */DCL D32      LIT   '"32"';  /* DEVICE 32 DIGITAL IO MULTIPLEXOR    */DCL D33      LIT   '"33"';  /*           DATA PORT                 *//* KEYBOARD INTERFACE DEVICE ADDRESSES */DCL DREG       LIT '"130"'; /* KEYBOARD INTERFACE DATA REGISTER    */DCL CREG       LIT '"131"'; /* KEYBOARD INTERFACE CONTROL REGISTER */DCL PULSE_EXWR LIT 'WRITE(CREG)="40"\1; WRITE(CREG)=1; WRITE(CREG)="100"\1'; /* MODE 1 WRITE THEN READ *//* GATE & TRIGGER DEVICES */DCL D164      LIT '"164"';DCL D165      LIT '"165"';/* FUNDAMENTAL SYNCLAVIER LITERALS: */DCL NUM.PARTIALS       LIT   '04';   /* NUMBER OF PARTIALS PER TIMBRE *//* **** NOTE ****  -  DURING RUN TIME,  THE KEYBOARD AND SPLIT KEYBOARD   AREA TREATED AS TRACKS IN MANY WAYS.   LOOK UP TABLES FOR TRACKS   ARE THEREFORE OFFSET BY 2 WORDS WHEN COMPARED TO THE TRACKS OF THE   SEQUENCER. */DCL NUM.KEYS           LIT   '(85)';         /* ALLOWABLE SOFTWARE KEY RANGE - 0-84  */DCL MAX.KEY#           LIT   '(NUM.KEYS-1)'; /* MAX ALLOWED KEY NUMBER */DCL SYNC.MIDDLE.C      LIT   '(36)';         /* KEY NUMBER OF MIDDLE C ON SYNCLAVIER */DCL MIDI.MIDDLE.C      LIT   '(60)';         /* KEY NUMBER OF MIDDLE C IN MIDI WORLD *//* RUN-TIME DATA STRUCTURE: */DCL ZERO.TIME          LIT   ' 50';     /* PLAY.TIME.LSB OF SCRIPT=0.000 */DCL RTE.MAX            LIT   '225';     /* LARGEST RTE PEDAL POSITION    */DCL DEFAULT.MIDI.VELOCITY LIT 'RTE.MAX'; /* MIDI DEFAULT VELOCITY        */DCL MAX.TRACKS         LIT   '256';     /* MAX # OF TRACKS PROCESSED   */DCL NUM.KBD.TRACKS     LIT   '  2';     /* NUM OF KEYBOARD TRACKS      */DCL MAX.INPUTS         LIT   '202';     /* NUM OF INPUT CHANNELS (KBD+MIDI) */DCL FIRST.LOD.TRACK    LIT   '202';     /* ABS. TRACK NUMBER OF FIRST LOD TRACK                  */DCL LAST.LOD.TRACK     LIT   '218';     /* ABS. TRACK NUMBER OF FIRST TRACK AFTER LAST LOD TRACK *//* VARIOUS IMPORTANT OR TEMP TRACKS USED BY PROCEDURES */DCL UNDO.TRK           LIT   '230';     /* ABS TRK USED BY G-SCREEN FOR UNDO FUNCTION */DCL UNDO.TMP.TRK       LIT   '231';     /* ABS TRK USED BY G-SCREEN FOR UNDO FUNCTION */DCL BOUNCE.TRK         LIT   '240';     /* ABS TRK USED BY BOUNCE FOR TMP TRK */DCL CHAIN.TRK          LIT   '241';     /* ABS TRK USED BY CHAIN TO CHAIN ON TO */DCL SOU.TRK            LIT   '242';     /* ABS TRK USED BY CHAIN FOR COPY OF SOURCE TRK */DCL SOU.COPY           LIT   '243';     /* ABS TRK USED BY CUT FOR COPY OF SOURCE TRK */DCL TEMP.TRK           LIT   '244';     /* ABS TRK USED BY PASTE FOR TMP TRK */DCL ALWAYS.EMPTY.TRACK LIT   '248';     /* ABS. TRACK NUMBER OF AN ALWAYS EMPTY TRACK          */DCL LOCATE.INFO.TRACK  LIT   '249';     /* ABS. TRACK NUMBER WHERE AEE LOCATE POSITIONS STORED */DCL CUE.AUDITION.TRACK LIT   '254';     /* ABS. TRACK NUMBER OF CUE AUDITION TRK               */DCL RESERVED.TRACK     LIT   '255';     /* TRACK 255 IS RESERVED FOR FUTURE USE */DCL MAX.TIMBRES        LIT   '256';     /* MAX # OF TIMBRES PROCESSED  */dcl Bit0  lit '1';dcl Bit1  lit '2';dcl Bit2  lit '4';dcl Bit3  lit '8';dcl Bit4  lit '16';dcl Bit5  lit '32';dcl Bit6  lit '64';dcl Bit7  lit '128';dcl Bit8  lit '256';dcl Bit9  lit '512';dcl Bit10 lit '1024';dcl Bit11 lit '2048';dcl Bit12 lit '4096';dcl Bit13 lit '8192';dcl Bit14 lit '16384';dcl Bit15 lit '32768';/* 32 bit address literals */dcl lw#msb lit '0';  /* Upper 16 bits of 32-bit "long word" */dcl lw#lsb lit '1';  /* Lower 16 bits of 32-bit "long word" */dcl lw#ilt lit '-1'; /* 32 bit compare is less than         */dcl lw#ieq lit '0';  /* 32 bit compare is equal to          */dcl lw#igt lit '+1'; /* 32 bit compare is greater than      */dcl lw#num lit '0';	/* numerator in array stored here		*/dcl lw#den lit '1';	/* denominator in array stored here		*//* use with care - check parentheses carefully!   dcl is.negative	lit '& Bit15';   dcl is.odd			lit '& 1';*/