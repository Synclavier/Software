dcl swappable lit '';insert ':-xpl:asciilit';insert ':-xpl:termut';insert 'gdate';pdate: proc(date); /* print date */   dcl date   fixed; /* date to print */   dcl (i, j) fixed;   dcl months data ('JanFebMarAprMayJunJulAugSepOctNovDec'); /* month names */   if date <> 0 then do; /* a date of zero means no date */      pnum (date and "37", 0); /* print day */      wchar (a.sp);      j = ((shr(date, 5) and "17") - 1)*3; /* get index into month name array */      do i = 0 to 2; /* print month name */         wchar (byte(months, j + i));      end;      wchar (a.sp);      pnum (shr(date, 9), 2); /* print year */   end; /* of no date */end pdate;ptime: proc(time); /* print time */   dcl time fixed; /* time to print */   if time <> 0 then do; /* time of zero means no time */      pnum (shr(time, 11) and "17", 0); /* print hours */      wchar (a.colon);      pnum (shr(time, 5) and "77", 2); /* print minutes */      wchar (a.colon);      pnum (shl(time and "37", 1), 2); /* print seconds */      if time < 0 then pstring (' p.m.'); else pstring(' a.m.'); /* print AM/PM */   end; /* of no time */end ptime;call pdate (get_date); crlf;call ptime (get_time); crlf;