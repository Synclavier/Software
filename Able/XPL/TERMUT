/* TERMUT  $TITLE  Terminal Management Utilities *//* NON-INTERRUPT DRIVEN VERSION (TERMUT)             */module TERMINAL_UTILITIES;   configuration modelC, noMULDIV; /* configuration */   insert ':-xpl:literals';   insert ':-xpl:asciilit';   insert ':xpl:ttyio.s'; /* get terminal handling code (TTYIOI.S for interrupts, TTYIO.S for no interrupts) */   insert ':xpl:termcode';end TERMINAL_UTILITIES;