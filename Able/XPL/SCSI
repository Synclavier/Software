/* $Title SCSI Drivers Library *//***********************************************          S C S I   D R I V E R S                L I B R A R Y ***********************************************/module ScsiDrivers;   dcl PolyExt   lit '1';     /* Set to one to include poly and ext memory code (always set when SWAPping) */   dcl Swappable lit '';      /* Set to RECURSIVE SWAP to swap SCSI drivers (add ABORT.SCSI in SCSICONNECT also) */   Abort.Scsi: proc;          /* null procedure for SCSI */   end Abort.Scsi;   insert ':xpl:scsicode';end ScsiDrivers;