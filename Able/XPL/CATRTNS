/* CATRTNS  $title System Catalog Routines */module catalog_routines;   configuration modelC, noMULDIV;   dcl swappable lit ''; /* set to SWAP to swap catalog routines (SCSI to SCSISWAP!) */   insert ':-xpl:literals'; 	/* general literals */   insert ':-xpl:asciilit'; 	/* ASCII definitions */   insert ':-xpl:syslits'; 		/* system literals */   insert ':-xpl:tapelits'; 	/* tape literals */   insert ':-xpl:scsi'; 		/* SCSI device drivers */   insert ':-xpl:devutil'; 		/* device utilities */   insert ':-xpl:whendisk';   insert ':-xpl:intrprtr';      insert ':xpl:catcode'; /* catalog routine code */end catalog_routines;