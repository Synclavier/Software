/* TERMUTIL  $TITLE  Terminal Management Utilities *//* INTERRUPT DRIVEN VERSION (TERMUTIL)             */module TERMINAL_UTILITIES;   configuration modelC, noMULDIV; /* configuration */   insert ':-xpl:literals';   insert ':-xpl:asciilit';   insert ':xpl:ttyioi.s'; /* get terminal handling code (TTYIOI.S for interrupts, TTYIO.S for no interrupts) */   insert ':xpl:termcode';end TERMINAL_UTILITIES;