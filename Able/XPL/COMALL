#Create destination folder
if [ ! -e "${XPLFolder}/-XPL/LIBS" ]; then
	mkdir "${XPLFolder}/-XPL/LIBS"
fi

echo "Compiling -XPL Libraries"

#Compile -XPL Libraries
com :XPL:CATRTNS   		-of   :-XPL:LIBS:CATRTNS
com :XPL:CATSWAP    		-of   :-XPL:LIBS:CATSWAP
com :XPL:CATUTIL    		-of   :-XPL:LIBS:CATUTIL
com :XPL:DEVUTIL    		-of   :-XPL:LIBS:DEVUTIL
com :XPL:DYNAMIC    		-of   :-XPL:LIBS:DYNAMIC
com :XPL:FILEIO     		-of   :-XPL:LIBS:FILEIO
com :XPL:FILESWAP   		-of   :-XPL:LIBS:FILESWAP
com :XPL:INTRPRTR   		-of   :-XPL:LIBS:INTRPRTR
com :XPL:MESSAGES   		-of   :-XPL:LIBS:MESSAGES
com :XPL:OPTICAL    		-of   :-XPL:LIBS:OPTICAL
com :XPL:PLOTLIB    		-of   :-XPL:LIBS:PLOTLIB
com :XPL:RELDAT     		-of   :-XPL:LIBS:RELDAT
com :XPL:SCANARGS   		-of   :-XPL:LIBS:SCANARGS
com :XPL:SCSI       		-of   :-XPL:LIBS:SCSI
com :XPL:SCSICMDS   		-of   :-XPL:LIBS:SCSICMDS
com :XPL:SCSISWAP   		-of   :-XPL:LIBS:SCSISWAP
com :XPL:STRINGS	   	-of   :-XPL:LIBS:STRINGS
com :XPL:SYSLITS	   	-of   :-XPL:LIBS:SYSLITS
com :XPL:TAPEBRDG   		-of   :-XPL:LIBS:TAPEBRDG
com :XPL:TAPEFACE   		-of   :-XPL:LIBS:TAPEFACE
com :XPL:TAPELIB    		-of   :-XPL:LIBS:TAPELIB
com :XPL:TAPESCSI   		-of   :-XPL:LIBS:TAPESCSI
com :XPL:TERMUT     		-of   :-XPL:LIBS:TERMUT
com :XPL:TERMUTIL  		-of   :-XPL:LIBS:TERMUTIL
com :XPL:TTYIO      		-of   :-XPL:LIBS:TTYIO
com :XPL:TTYIOI     		-of   :-XPL:LIBS:TTYIOI
com :XPL:WHENDISK   		-of   :-XPL:LIBS:WHENDISK
