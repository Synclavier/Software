/* SOME-FM   $TITLE FLAGS FOR FM ONLY COMPILATION   Modified:   10/16/89 - cj  - added inc.creg for inclusion of d130 (creg/dreg)                    code.   Only set to 0 for debugging. inc.fm for                    fm code as well.   06/15/89 - PF  - Removed dcl of inc.new.midi   06/05/89 - TSS - Added INC.KBD.MAPS and INC.KBD.MAP.SCREEN   01/13/89 - PF  - added inc.new.midi for Monster MIDI   09/01/88 - SGS - Added inc.pvoc   08/29/88 - SGS - Added inc.clav   09/28/88 - LSS - created this version      This file is used to replace :synlits:somelits*/configuration modelc,muldiv;    dcl new.pan            lit '1';    /* VELOCITY KBD      */dcl inc.poly           lit '0';    /* POLY              */dcl inc.mono           lit '0';    /* NO MONO           */dcl mono.lb            lit '0';    /* NO LARGE BUFFERS  */dcl inc.axe            lit '0';    /* NO GUITAR         */dcl inc.dtd            lit '1';    /* set to 1 to include direct-to-disk */dcl inc.midi           lit '1';    /* set to 1 to include midi */dcl inc.mouse          lit '1';    /* set to 1 to include mouse code   */dcl inc.mult           lit '1';    /* set to 1 to include multichannel */dcl inc.music.notation lit '1';    /* include m. nota   */dcl inc.stm            lit '1';    /* set to 1 to include sample-to-memory */dcl inc.stmm           lit '1';    /* set to 1 to include rel m stm commands */dcl inc.tds            lit '1';    /* set to 1 to include timbre display */dcl inc.clav           lit '1';    /* set to 1 to include clavier links */dcl inc.creg           lit '1';    /* set to 1 to include d130 code     */dcl inc.fm             lit '1';    /* set to 1 if d160-d163 are avail   */dcl inc.pvoc           lit '1';    /* set to 1 if poly voice cards, set to 0 if mono voice cards */dcl inc.pro            lit '1';    /* set to 1 to include RS232 protocol */dcl inc.kbd.maps       lit '1';    /* set to 1 to use new kbd mapping code */dcl inc.kbd.map.screen lit '(0 & inc.kbd.maps)'; /* set to 1 bring in kbd mapping screen too */