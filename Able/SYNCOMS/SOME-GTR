/*	:SYNCOMS:SOME-GTR  $TITLE  FLAGS FOR VK GUITAR COMPILATION//	Note: This file is used to replace :SYNLITS:SOMELITS at compile timeModified:1991/02/07 - MWH - Combine 3200, 6400 and 9600 RTP's1989/10/16 - cj  - added inc.creg for inclusion of d130 (creg/dreg)						 code.  Only set to 0 for debugging. inc.fm for						 fm code as well.1989/06/15 - PF  - Removed dcl of inc.new.midi1989/06/05 - TSS - Added INC.KBD.MAPS and INC.KBD.MAP.SCREEN1989/01/13 - PF  - added inc.new.midi for Monster MIDI1988/09/01 - SGS - Added inc.pvoc1988/08/29 - SGS - Added inc.clav1988/06/06 - MWH - Rename from "SYN-NGTR"*/configuration modelc,muldiv;    dcl new.pan            lit '1';    /* VELOCITY KBD          */dcl inc.poly           lit '1';    /* POLY                  */dcl inc.mono           lit '0';    /* NO MONO               */dcl mono.lb            lit '0';    /* NO LARGE BUFFERS      */dcl inc.axe            lit '1';    /* GUITAR                */dcl inc.slink          lit '0';    /* SLINK - SCSI TARGET   */dcl inc.dtd            lit '1';    /* set to 1 to include direct-to-disk */dcl inc.midi           lit '1';    /* set to 1 to include midi */dcl inc.mouse          lit '1';    /* set to 1 to include mouse code   */dcl inc.mult           lit '1';    /* set to 1 to include multichannel */dcl inc.music.notation lit '1';    /* include m. nota   */dcl inc.stm            lit '1';    /* set to 1 to include sample-to-memory */dcl inc.stmm           lit '1';    /* set to 1 to include rel m stm commands */dcl inc.tds            lit '1';    /* set to 1 to include timbre display */dcl inc.creg           lit '1';    /* set to 1 to include d130 code     */dcl inc.fm             lit '1';    /* set to 1 if d160-d163 are avail   */dcl inc.pro            lit '1';    /* set to 1 to include RS232 protocol */dcl inc.kbd.maps       lit '1';    /* set to 1 to use new kbd mapping code */