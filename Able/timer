/* Able CPU timing test */dcl (a,b,c)  fixed;dcl buf(256) fixed;dcl cuf(256) fixed;print 'Able CPU Timing Test.';print;print 'Press return to start 10,000,000 loops--',;a = read(40);print;write(40) = 7;do a = 0 to 9999;   do b =  0 to 999;      c = core(a) + core(b);   end;end;write(40) = 7;print 'Completed.';print;print 'Press return to time 32 meg memory import--',;a = read(40);print;write(40) = 7;a = 1;while (a != 0){	write("313") = addr(buf(0));	write("60")  = 99;	write("10")  = 255;	write("373") = read("63");	a = a + 1;}write(40) = 7;print 'Completed.';print;print 'Press return to time 32 meg memory copy--',;a = read(40);print;write(40) = 7;a = 1;while (a != 0){		write("313") = addr(buf(0));	write("300") = addr(cuf(0));	write("10")  = 255;	write("373") = read("360");	a = a + 1;}write(40) = 7;print 'Completed.';print;