/* DBUGSOU - debug routines *//* 02/20/90 - cj - added xon/xoff check */module dbugmod;   dcl break.me  fixed PUBLIC;   dcl newinfo   fixed PUBLIC;   dcl ddutrk    fixed PUBLIC;  /* variables to read data from the     */   dcl ddusec    fixed PUBLIC;  /* disk subsystem and transfer out     */   dcl ddulen    fixed PUBLIC;  /* the scsi port for sfm debugging     */   dcl xtrack    fixed PUBLIC;  /*    track for debug output           */       xtrack  =   (-1);   when ttiint then begin;      if rcvdcharacter = 0 then do;         /* could set break.me for exit */         return;      end;      if rcvdcharacter = "23" then do; /* xoff */         do while read("50")<>"21";end;         return; /* ignore xon/xoff */      end;      if rcvdcharacter = "21" then return;      newinfo = rcvdcharacter;  /* hello? */   end;end dbugmod;