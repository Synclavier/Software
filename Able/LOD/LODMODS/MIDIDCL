/* MIDIDCL - midi routines (for signal meter) */dcl init.midi.subsystems  proc              EXTERNAL;dcl init.signal.display   proc              EXTERNAL;dcl set.track.display     proc(fixed,fixed) EXTERNAL;dcl set.level.display     proc(array)       EXTERNAL;dcl Initialize.Signal.Meter.Lookup proc     EXTERNAL;dcl input.led      lit ' 1';dcl record.led     lit ' 2';dcl ready.led      lit ' 4';dcl track#.red     lit ' 8';dcl track#.green   lit '16';library ':lod:lodlibs:midilib';