/* CUEDCL - basic routines for processing cues in external memory */dcl num.of.alloc     fixed   EXTERNAL; /* number of entries in the allocation # */                                       /* table that are used                   */dcl max.store.secs   fixed EXTERNAL;   /* defines ext memory limit for cues     */dcl max.store.disk   fixed EXTERNAL;   /* defines disk storage limit for cues   */dcl cue.data.s#      array EXTERNAL;   /* defines start of audio data on disk   */dcl num.of           array EXTERNAL;   /* number of entries     */dcl cur.ix           array EXTERNAL;   /* current dtd index     */dcl cue.num          fixed EXTERNAL;   /* # of cue records in data base                   */dcl cue.numsec       fixed EXTERNAL;   /* num of full sectors of cue defs                 */dcl cue.numwrd       fixed EXTERNAL;   /* num or extra words of cue defs                  */dcl sort.base        data  EXTERNAL;dcl zero.cue.dir     proc  EXTERNAL;dcl current.cue      array EXTERNAL;dcl current.dtd.cue# fixed EXTERNAL;  /* id # of most recent synclavier cue */dcl cue.sec          fixed EXTERNAL;  /* holds pointer to cue in ext mem set */dcl cue.wrd          fixed EXTERNAL;  /* by get .cue.ptrs                    */dcl get.cue.ptrs     proc(fixed)             EXTERNAL; /* look up cue pointers from alloc #   */dcl get.sort.cue     proc(fixed,fixed)       EXTERNAL; /* look up nth sorted cue (time.sec#,alph.sec#,smpt.sec#) */dcl read.basic.cue   proc(fixed,fixed,array) EXTERNAL; /* read basic cue info into array */ dcl Or.Cue.Track.Bits proc(fixed,fixed,array) EXTERNAL;/* get list of all tracks in cue */dcl write.entire.cue proc(fixed,fixed,fixed) EXTERNAL; /* write a cue to memory */dcl check.name       proc(array,array)       EXTERNAL; /* look for name match */dcl clean.cue.name   proc(array)             EXTERNAL;dcl found.cue.num fixed EXTERNAL;  /* index entry of item (or place to insert) */dcl found.cue.id# fixed EXTERNAL;  /* id#         of item (only if match     ) */dcl perform.binary.search proc(array,array,fixed) EXTERNAL;  /* perform binary search */dcl get.alloc.num         proc EXTERNAL;                     /* look for free alloc #       */dcl cue.is.in.song.area    proc(array,array,array) EXTERNAL;dcl cue.is.in.current.song proc(array)             EXTERNAL;dcl cue.is.in.command.song proc(array)             EXTERNAL;dcl cue.info.updated fixed EXTERNAL; /* set = 1 to rewrite cue info to disk */dcl insert.cue.in.list proc(fixed,fixed,fixed) EXTERNAL;  dcl any.cues.deleted fixed EXTERNAL;dcl process.all.cues proc(fixed,fixed,fixed)  EXTERNAL;  /* routine to process all cues and perform an operation on them */dcl lookup.cur.cue   proc(fixed)              EXTERNAL;dcl find.cue.indexes proc(fixed,array,fixed)  EXTERNAL;dcl delete.cue       proc(fixed,fixed,fixed)  EXTERNAL;  /* pass id#,  1 to garbage collect,  1 if about to replace */dcl convert.basic.record.to.msecs    proc(array) EXTERNAL; /* convert sample ptrs to msecs */dcl convert.entire.record.to.msecs   proc(fixed) EXTERNAL; /* convert to absolute sample #'s */dcl Compute.Absolute.S#              proc(array) EXTERNAL;dcl convert.entire.record.to.samples proc(fixed) EXTERNAL; /* convert to absolute sample #'s */library ':LOD:LODLIBS:CUELIB';