/* DSPLITS - Literals for the DSP, TSM, Wave Display *//* DSP state literals */dcl dsp#off          lit '0';     /* None                               */dcl dsp#load.boot    lit '1';dcl dsp#load.alg     lit '2';dcl dsp#load.data    lit '3';dcl dsp#working      lit '4';dcl dsp#abort        lit '5';dcl comp#init        lit  '0';     /* no compressing                     */dcl comp#working     lit  '1';     /* working on a compression           */dcl comp#done        lit  '2';     /* compressed data is ready to send   *//* literals for different DSP functions */dcl dsp#idle         lit  '0';dcl dsp#boot         lit  '1';dcl dsp#tsm          lit  '2';dcl dsp#check.dsp70  lit  '3';dcl dsp#SynSCSI      lit  '4';     /* reading or writing the Synclavier SCSI bus *//* literals for different DSP algorithms */dcl dsp#three        lit  '0';dcl dsp#one          lit  '1';/* TSM algorithms */dcl dsp#two          lit  '2';dcl dsp#nosrc        lit  '0';/* Sample Rate Conversion Algorithms */dcl dsp#src          lit  '1';/* Erata that makes life more bearable */dcl dsp#left         lit  '0';dcl dsp#right        lit  '1';/* Literals for DSP70 servicing       */dcl dsp#event.mask       lit '"h18"';dcl dsp#read.audio       lit '"h08"';dcl dsp#write.audio      lit '"h10"';dcl dsp#done             lit '"h18"';dcl dsp#extended.cmd     lit '"h18"';dcl dsp#write.ack.set    lit '"h92"';/* init 56k port, set hf1, set treq   */dcl dsp#write.ack.clr    lit '"h82"';/* init 56k port, clr hf0,1, set treq */dcl dsp#read.ack.set     lit '"h89"';/* init 56k port, set hf0, set rreq   */dcl dsp#read.ack.clr     lit '"h81"';/* init 56k port, clr hf0,1, set rreq */dcl dsp#ext.ack.set      lit '"h9a"';/* init 56k port, set hf1,0, set treq */dcl DSPSel      lit  '"70"';dcl DSPCtl      lit  '"71"';dcl DSPData     lit  '"72"';dcl DSPEnable   lit  '"40"';dcl DSPReset    lit '"200"';dcl DSP#        lit  '"00"';dcl ICR         lit  '"00"';dcl CVR         lit  '"01"';dcl ISR         lit  '"02"';dcl IVR         lit  '"03"';dcl DSP16SE     lit  '"44"';dcl DSP16ZF     lit  '"45"';dcl DSP32SE     lit  '"46"';dcl DSP32ZF     lit  '"47"';dcl DSPRead     lit  '"00"';dcl DSPWrite    lit  '"10"';/* Literals for Syn.SCSI.xfer */dcl SyS#off    lit  '0';dcl SyS#OptDTD lit  '1';/* transfer from Optical Disk to DTD */dcl SyS#DTDOpt lit  '2';/* transfer from DTD to Optical      */dcl LaserDev   lit   '10';/* psuedo-device number for the optical     */dcl DTDDev     lit   '14';/* psuedo-device number for the optical     */