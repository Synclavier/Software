/* Literals to access m  sb/lsbs of 32-bit variables */dcl (play.time.msb)      lit  'Play.Time(0)';        /* every 200 msec over scsi    */dcl (play.time.lsb)      lit  'Play.Time(1)';        /* bus                         */dcl (play.time.acu)      lit  'Play.Time(2)';dcl (mark.button.msb)    lit  'Mark.Button(0)';      /* mark start point        */dcl (mark.button.lsb)    lit  'Mark.Button(1)';dcl (mark.button.acu)    lit  'Mark.Button(2)';dcl (mark.button.s#.msb) lit  'Mark.Button.S#(0)';   /* that corresponds   */dcl (mark.button.s#.lsb) lit  'Mark.Button.S#(1)';   /* to Mark.Button     */dcl (sync.time.msb)      lit  'Sync.Time(0)';dcl (sync.time.lsb)      lit  'Sync.Time(1)';dcl (cur.dtd.ms.msb)     lit 'Cur.Dtd.Ms(0)';dcl (cur.dtd.ms.lsb)     lit 'Cur.Dtd.Ms(1)';dcl sync.point.disk.msb  lit 'Sync.Point.Disk(0)';dcl sync.point.disk.lsb  lit 'Sync.Point.Disk(1)';dcl sync.point.trig.msb  lit 'Sync.Point.Trig(0)';dcl sync.point.trig.lsb  lit 'Sync.Point.Trig(1)';dcl sync.point.used.disk.msb lit 'Sync.Point.Used.Disk(0)';dcl sync.point.used.disk.lsb lit 'Sync.Point.Used.Disk(1)';dcl sync.point.used.trig.msb lit 'Sync.Point.Used.Trig(0)';dcl sync.point.used.trig.lsb lit 'Sync.Point.Used.Trig(1)';dcl seg.sync.msb lit 'Seg.Sync(0)';dcl seg.sync.lsb lit 'Seg.Sync(1)';dcl seg.disk.msb lit 'Seg.Disk(0)';dcl seg.disk.lsb lit 'Seg.Disk(1)';dcl seg.end.msb  lit 'Seg.End (0)';dcl seg.end.lsb  lit 'Seg.End (1)';dcl seg.time.msb lit 'Seg.Time(0)';dcl seg.time.lsb lit 'Seg.Time(1)';/* literals for access to msb, lsb of cue/song variables */dcl (song.data.msb)          lit 'Song.Data(0)';dcl (song.data.lsb)          lit 'Song.Data(1)';dcl (song.len.msb)           lit 'Song.Length(0)';dcl (song.len.lsb)           lit 'Song.Length(1)';dcl (song.end.msb)           lit 'Song.Ending(0)';dcl (song.end.lsb)           lit 'Song.Ending(1)';dcl (song.data.s#.msb)       lit 'Song.Data.S#(0)';dcl (song.data.s#.lsb)       lit 'Song.Data.S#(1)';dcl (song.end.s#.msb)        lit 'Song.End.S#(0)';dcl (song.end.s#.lsb)        lit 'Song.End.S#(1)';dcl (c.data.msb)             lit 'C.Data(0)';       /* start of command.song  */dcl (c.data.lsb)             lit 'C.Data(1)';dcl (c.len.msb)              lit 'C.Len(0)';       /* length of command.song */dcl (c.len.lsb)              lit 'C.Len(1)';dcl (c.end.msb)              lit 'C.End(0)';       /* ending sector #        */dcl (c.end.lsb)              lit 'C.End(1)';dcl (c.data.s#.msb)          lit 'C.Data.S#(0)';       /* starting sample #      */dcl (c.data.s#.lsb)          lit 'C.Data.S#(1)';dcl (c.end.s#.msb)           lit 'C.End.S#(0)';dcl (c.end.s#.lsb)           lit 'C.End.S#(1)';dcl (syncl.time.msb)         lit 'Syncl.Time(0)';dcl (syncl.time.lsb)         lit 'Syncl.Time(1)';dcl (syncl.time.acu)         lit 'Syncl.Time(2)';dcl (scroll.s#.msb)          lit 'Scroll.S#(0)';dcl (scroll.s#.lsb)          lit 'Scroll.S#(1)';dcl (scroll.base.msb)        lit 'Scroll.Base(0)';dcl (scroll.base.lsb)        lit 'Scroll.Base(1)';dcl (last.cue.end.msb)       lit 'Last.Cue(0)';dcl (last.cue.end.lsb)       lit 'Last.Cue(1)';