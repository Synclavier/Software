/* DBUTLITS - literals to control inclusion of debug code *//* NOTE: all flags should be set to 0 for normal released software */dcl hzdbug      lit '0';        /* 1: print out synchronization info      */dcl xdebug      lit '0';        /* 1: will cause printout of all i/o      */                                /*    commands for XTRACK                 */dcl xdebugreset lit '0';        /* 1: cause extraneous reset for error recovery testing */dcl tdebug      lit '0';        /* 1: will cause printout of all i/o      */                                /*    commands that have a nonzero status */dcl dspdebug    lit '0';        /* include dsp  debug code                */dcl optdebug    lit '0';        /* include optical debug code             */dcl fastboot    lit '0';        /* provide fast startupd during debug     */