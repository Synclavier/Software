/* LOD1   $TITLE  Main operating system software for the Direct-to-Disk */configuration modelc,muldiv;// Release Date, Revisiondcl version data         ('8.8 - 3/06/15');dcl inc.max.tracks       lit ' 1';   /* allow max tracks if hardware is */                                     /* available.                      */pdl 512;insert ':lod:lodlits:globlits';		 /* for configuration model			*/insert ':lod:lodmods:maindcl';       /* get main routine                */call MainLoop(version, inc.max.tracks);