/* debugging routines for DSP */begin;   if newinfo = 98 then do;/* "b" toggle dspdebug */      if dspdebug = 0 then do;         dspdebug = 1;         print 'DSPDebug = on';      end;      else do;         dspdebug = 0;         print 'DSPDebug = off';      end;   end;   else if newinfo = 114 then do;/* "r" reselect */      dsp.paused = 1;      print 'Reselecting Synclavier';      call Reselect.Synclavier(0,misc.buf);   end;end;