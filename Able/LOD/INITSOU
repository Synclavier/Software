/* INITSOU - Basic initialization routine for LOD Operating System *//* Also includes basic disk/tape io items                          */module initmod;   insert ':-xpl:scsilits';   insert ':-xpl:asciilit';   insert ':lod:lodlits:globlits'; /* get glob literals                */   insert ':lod:lodlits:xmemlits'; /* literals for xmem allocation     */   insert ':lod:lodlits:dbuglits'; /* get dbug literals                */   insert ':lod:lodlits:polylits'; /* literals for poly synth          */   insert ':lod:lodlits:cuelits';  /* literals for cue storage         */   insert ':lod:lodlits:ddtlits';  /* literals for DDT hardware        */   insert ':lod:lodlits:initlits'; /* literals for init/SCSI           */   insert ':lod:lodlits:jazlits';  /* literals for jaz backup		   */   insert ':lod:lodmods:miscdcl';  /* get misc routines                */   insert ':lod:lodmods:dbugdcl';  /* debug routines                   */   insert ':lod:lodmods:polydcl';  /* poly synth routines              */   insert ':lod:lodmods:mididcl';  /* midi routines                    */   insert ':lod:lodmods:cuedcl';   /* basic cue routines               */   insert ':lod:lodmods:ddtdcl';   /* basic ddt routines               */   insert ':lod:initsoua';   insert ':lod:initsoub';   insert ':lod:initsouc';   insert ':lod:initsoud';   insert ':lod:initsoue';   insert ':lod:initsouf';end initmod;