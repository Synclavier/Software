/* TALKSOU - Module for talking between LOD and SYNTABULATOR */module talkmod;   insert ':-xpl:scsilits';   insert ':lod:lodlits:globlits'; /* get glob literals                */   insert ':lod:lodlits:dbuglits'; /* get dbug literals                */   insert ':lod:lodlits:varlits';  /* variable msbs/lsbs               */   insert ':lod:lodlits:polylits'; /* literals for poly synth          */   insert ':lod:lodlits:xmemlits'; /* literals for xmem allocation     */   insert ':lod:lodlits:cuelits';  /* literals for cue storage         */   insert ':lod:lodlits:ddtlits';  /* literals for ddt routines        */   insert ':lod:lodlits:dsplits';  /* literals for DSP items           */   insert ':lod:lodlits:initlits'; /* literals for init/SCSI           */   insert ':lod:lodlits:stacklit'; /* literals for cue stacking        */   insert ':lod:lodlits:talklits'; /* literals for communications      */   insert ':lod:lodmods:miscdcl';  /* get misc routines                */   insert ':lod:lodmods:dbugdcl';  /* debug routines                   */   insert ':lod:lodmods:polydcl';  /* poly synth routines              */   insert ':lod:lodmods:cuedcl';   /* basic cue routines               */   insert ':lod:lodmods:ddtdcl';   /* basic ddt routines               */   insert ':lod:lodmods:initdcl';  /* scsi variables                   */   insert ':lod:lodmods:dspdcl';   /* dsp items                        */   insert ':lod:lodmods:stackdcl'; /* real time cue stack              */   insert ':lod:lodmods:maindcl';  /* get some items from main         */   insert ':LOD:TALKSOUA' ;          /* lod/syncl talk     */   insert ':LOD:TALKSOUB' ;          /* error message      */   insert ':LOD:TALKSOUC' ;          /* lod/syncl communic */   insert ':LOD:TALKSOUD' ;          /* lod/syncl answer   */   insert ':LOD:TALKSOUE' ;          /* lod commands       */   insert ':LOD:TALKSOUF' ;          /* Optical disk       */end talkmod;