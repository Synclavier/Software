dcl (a,b,c,d,e) floating;a=190;dcl list floating data  (   +15.0,                      /* clipping */   +12.0,   + 8.0,   + 4.0,   + 2.0,   + 0.0,   - 2.0,   - 4.0,   - 7.0,   -10.0,   -16.0,   -22.0,   -28.0,   -34.0,   -40.0,   -46.0,   -52.0,   -58.0);do b=0 to 17;   c=list(17-b);   print a+(c-15)*8/6;         /* data is 8*log2.  6 db = factor of 2 */end;