echo "Compiling Mouse Real Time Module (MOUSEMOD:MOUSRSOU)"
com :synsou:mousemod:mousrsou -of :synlibs:mousrlib

echo "Compiling Mouse Function Module (MOUSEMOD:MOUSFSOU)"
com :synsou:mousemod:mousfsou -of :synlibs:mousflib

echo "Compiling Synclavier Mouse Cursor Module (MOUSEMOD:MOUSSSOU)"
com :synsou:mousemod:mousssou -of :synlibs:mousslib
