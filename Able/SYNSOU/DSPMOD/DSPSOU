/* DSPSOU  $TITLE  Routines for interfacing with the DSP board   Modified:   12/13/88 - cj  - changed 'smart' to 'speech'   11/16/88 - sjs - added debug stuff   07/08/88 - MWH - Add edit list file   */Module DSPMOD;dcl use.poly  lit '(1 & (INC.POLY<>0))'; /* set to 1 to use poly mem,  0 to use ext mem */insert ':-xpl:scsilits';insert ':-xpl:catrout';insert ':-xpl:intrprtr';insert ':synlits:comlits';insert ':synlits:globlits';insert ':synlits:fcodlits';insert ':synlits:asciilit';insert ':synlits:samplits';/* for max.symbols */insert ':synmods:ttydcl';insert ':synmods:globdcl';insert ':synmods:mathdcl';insert ':synmods:polydcl';insert ':synmods:xmemdcl';insert ':synmods:filedcl';insert ':synmods:tprimdcl';insert ':synmods:scrndcl';insert ':synmods:steddcl';insert ':synmods:stutldcl';insert ':synauxs:dsplits';insert ':synsou:dspmod:tsmsou';       /* Time Scale Modification routines */insert ':synsou:dspmod:tsmsou1';      /* newer Time Scale Modification routines */insert ':synsou:dspmod:bugsou';       /* Debug routines */insert ':synsou:dspmod:pitchsou';     /* Pitch Detection routine *//*Pitch.Detect: proc (base,time) public swapable;   dcl base    array;   dcl time    array;end Pitch.Detect;*/SR.Convert: proc (srate) public swapable;   dcl srate   fixed;end SR.Convert;end DSPMOD;