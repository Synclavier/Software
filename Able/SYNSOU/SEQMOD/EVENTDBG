/* Event Debugging Routines */dcl Print.Event  proc(array) external;begin;   insert ':synauxs:seqlits';   print.event: proc (event) public recursive swap;      dcl event array;      disable;      print 'Event Track      ', event(event.track#);      print 'Event Time       ', event(event.time.msb), event(event.time.lsb), event(event.time.count);      print 'Event Type       ', event(event.type);      print 'Event W1 - W4    ', octal(event(event.w1)),'  ',octal(event(event.w2)),'  ',octal(event(event.w3)),'  ',octal(event(event.w4));      print 'Event Key#       ', event(event.key#);      print 'Event Cue Id     ', event(event.cue.id);      print 'Event Duration   ', event(event.duration.msb), event(event.duration.lsb);      print 'Event Pri-out-vol', event(event.priority), event(event.output), event(event.volume), event(event.pan);      print 'Event Cue Name   ', string(loc(addr(event(event.cue.name))));      print 'Event Cue In     ', event(event.in.msb), event(event.in.lsb);      print 'Event Cue Out    ', event(event.out.msb), event(event.out.lsb);      print 'Event Mark       ', event(event.mark.msb), event(event.mark.lsb);      print 'Event Fade In/Out', event(event.fade.in), event(event.fade.out);      print 'Event Bits       ', event(event.control.bits);      print 'Event Spares     ', event(event.spare1), event(event.spare2), event(event.spare3), event(event.spare4), event(event.spare5);      print 'Event Caption    ', string(loc(addr(event(event.caption))));   end print.event;end;