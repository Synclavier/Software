/* GIDTEST  $TITLE  Demo Program to Show Use of GIDMOD */insert ':synlits:comlits';insert ':synlits:globlits';insert ':synmods:giddcl';insert ':synmods:syndummy';dcl i fixed;print 'Current PID/GID area length in words: ',PID.GID.Len;print;/* save squares of first 10 integers */do i=1 to 10;   call PID(i-1,i*i);end;/* print them out in reverse order */i=10;do while i<>0;   print GID(i-1);   i=i-1;end;