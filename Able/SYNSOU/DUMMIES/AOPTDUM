module aoptdum;dcl Optical.Group                  fixed public;dcl Optical.Row                    fixed public;dcl Optical.Size                   fixed public;dcl Optical.Window.Grp             fixed public;dcl Opt.Caption.Locked             fixed public;dcl Opt.Category.Locked            fixed public;dcl Optical.Place.Track.Num        fixed public;dcl Optical.Place.Time             fixed public;dcl Optical.Audition.Button        fixed public;dcl Optical.Cue.Name               fixed public;dcl Optical.Capt.Type.Obj          fixed public;dcl Optical.Caption.String         fixed public;dcl Optical.Cat.Type.Obj           fixed public;dcl Optical.Category.Table         fixed public;Open.Optical.Panel             :proc (x) public; dcl x fixed; end;Close.Optical.Panel            :proc public; end;Update.Optical.Panel           :proc public; end;Get.Optical.Transfer.Event     :proc (x) public; dcl x fixed; end;Split.String                   :proc (x,y,z) public; dcl x array; dcl y fixed; dcl z array;end;Get.Default.Category           :proc (x) public; dcl x array; end;Stuff.Category.Into.Table      :proc (x,y) public; dcl x array; dcl y fixed; end;end aoptdum;