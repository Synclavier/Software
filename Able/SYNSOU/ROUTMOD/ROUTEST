/* TDSTEST - $Title Test Program for Numeric Timbre Display */insert ':synlits:comlits';          /* pdl size, configuration       */insert ':synmods:syncldcl';         /* bind in real time code        */insert ':synmods:mousedcl';         /* test program mouse routine    */insert ':synmods:ttydcl';           /* for ps                        */insert ':synmods:scrndcl';          /* for erase.rest.of.line        */insert ':synmods:routdcl';          /* multichannel and midi */insert ':synmods:dtddcl';           /* get direct-to-disk display screens  */call ps ('Routing Display Screen Test ');call pcr;dcl i fixed;call initialization;                /* real time initialization      */call mouse.init;                    /* mouse     initialization      */do while i<>(-1);   call erase.rest.of.line(23,0);   call cpos(23,0);   call ps('Enter Test Code --');   call tty_wait;   input i;   do case i;      call MULTICHANNEL.DISPLAY(0);      call MIDI.DISPLAY(0);      call DIRECT.TO.DISK.SONG.DIRECTORY(0);      call DIRECT.TO.DISK.TRACK.DISPLAY(0);   end;end;