insert ':synlits:comlits';insert ':synlits:globlits';insert ':synmods:tformdcl';insert ':synmods:syndummy';dcl str(64) fixed;dcl  i      fixed;disable;do while 1;   do i=0 to 299;      call conv.fixed.to.str(i,2,10,cf#unsigned\cf#erase\cf#right\cf#spfill,str);      print i,' --> "',string(str),'"';   end;end;