/* AUDMOD - Audio Event Editor Test Program */insert ':synlits:comlits';          /* pdl size, configuration       */insert ':synmods:syncldcl';         /* bind in real time code        */insert ':synmods:globdcl';insert ':synmods:mousedcl';         /* test program mouse routine    */insert ':synmods:ttydcl';           /* for ps                        */insert ':synmods:scrndcl';          /* for erase.rest.of.line        */insert ':synmods:loddcl';           insert ':synmods:linkdcl';          /* for Sync mode routines        */insert ':synmods:auddcl';           /* get our test screen           */dcl O$VOLUME_NAME (8) fixed public; /* band-aid */call initialization;                /* syncl rtp initialize          */call Mouse.Init;                    /* Initialize mouse              */call AUDIO.EVENT.EDITOR.SCREEN(0);call LEAVE.SYNCLAVIER.PROGRAM;