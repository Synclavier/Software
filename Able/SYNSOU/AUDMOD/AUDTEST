/* AUDMOD - Audio Event Editor Test Program (no real time code) */insert ':synlits:comlits';          /* pdl size, configuration       */insert ':synlits:globlits';insert ':synmods:globdcl';insert ':synmods:mousedcl';         /* test program mouse routine    */insert ':synmods:ttydcl';           /* for ps                        */insert ':synmods:scrndcl';          /* for erase.rest.of.line        */insert ':synmods:loddcl';           insert ':synmods:syndumm1';         /* dummy if no real time code    */insert ':synmods:auddcl';           /* get our test screen           */BEGIN;   RUN.SYN.LOOP:  PROC   PUBLIC;       dcl (save.mam,save.mal) fixed;      save.mam = read("60");      save.mal = read("61");      call main.loop;              /* this does the lod scan */      write("60") = save.mam;      write("61") = save.mal;   END;    END;call Mouse.Init;                    /* Initialize mouse */call AUDIO.EVENT.EDITOR.SCREEN(0);