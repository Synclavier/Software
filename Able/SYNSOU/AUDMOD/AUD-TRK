????????? Check this outOpen.Track.Window: proc swapable;   dcl i fixed;   dcl U#MTYPE fixed;   dcl U#TABLE fixed;   call Define.Graphics.Box(S#AutoAllocate,gpos.x(-6),gpos.y(900),gpos.x(8010),gpos.y(2200),1);   U#MTYPE = Define.Label     (S#AutoAllocate,10,70,10,'');    U#TABLE = Define.Table     (S#AutoAllocate,1,13,U#MTYPE);      /* Table using MTYPE */   call Fill.String.Table(U#TABLE,0,0,0,' Bounce| Lift| Open| Filter| Tweek| Nibble| Halt| Zip| Shine| Grind| Demolish| Quit');   i = Define.Scroll.Bar(S#Autoallocate, 1,1,863,gpos.y(900),gpos.y(2200));   call Activate.Scroll.Bar(i,0);   i = Define.32Bit.MSEC(S#AutoAllocate,11,0,14,O#MSEC.SMPTE,cf#default,cf#default);   call Define.Table(S#AutoAllocate,1,12,i);   vector_mode;   call line_type(1);   do i = gpos.y(2200) to gpos.y(1000) by gpix.y(100);      call connect(gpos.x(7000),i,gpos.x(8010),i);   end;   call line_type(0);   transparent_mode;   call Set.Window(0,0,10,22);    /* Create a full screen window */   call Select.Window(0);         /* Select full screen */   do i = 0 to 15;                /* Initialize track master to 1:1 correlation */      track.master(i) = i+1;   end;end Open.Track.Window;Open.Cue.Window: proc swapable;   dcl gbase             lit '500';  /* Transparent mode line at which to open window */   dcl base              lit '5';   dcl Cue.menu.Type     fixed;   dcl Cue.Menu          fixed;   dcl Drive.Type        fixed;   dcl Drive.Table       fixed;   dcl Drive.Parm.Type   fixed;   dcl Drive.Parm.Table  fixed;   dcl Cue.Name          fixed;   dcl Cue.In.Time       fixed;   dcl Cue.Out.Time      fixed;   dcl Cue.Dur.Time      fixed;   dcl Cue.Mrk.Time      fixed;   dcl Cue.Lines data (O#ICON.Connect, 5300, -(gbase-100), 5300, -(gbase+300),                       O#ICON.Connect, 7300, -(gbase-100), 7300, -(gbase+300),                       O#ICON.Connect,  -10, -gbase, 5300, -gbase,                       O#ICON.LineType,   1,                       O#ICON.Connect, 7300, -gbase  ,     8010, -gbase,                       O#ICON.Connect, 7300, -(gbase+100), 8010, -(gbase+100),                       O#ICON.Connect, 7300, -(gbase+200), 8010, -(gbase+200),                       O#ICON.Connect, 7300, -(gbase+300), 8010, -(gbase+300),                       O#ICON.LineType,   0,                       O#ICON.End);   call Define.Graphics.Box(S#AutoAllocate,gpos.x(-10),gpos.y(gbase-100),gpos.x(8010),gpos.y(gbase+300),1);   call Define.Graphics.Icon(S#AutoAllocate,0,0,0,Cue.Lines);   call Set.Default.Attr(0,ATTR#Normal);   call Set.Default.Attr(1,ATTR#Reverse\ATTR#AltIntensity);   call Set.Default.Attr(0,ATTR#AltIntensity);      call Define.Label(S#AutoAllocate,base  ,1,0,'Cue');      call Define.Label(S#AutoAllocate,base+1,1,0,'Sou');      i = Define.Switch(S#AutoAllocate,base+2,1,3,0,'Vol|RI/|RO\|Prt');      call Deposit.Switch(i,1);      i = Define.Switch(S#AutoAllocate,base+3,1,3,0,'Vol|RI/|RO\|Prt');      call Deposit.Switch(i,2);      call Define.Label(S#AutoAllocate,base  ,54,0,' In');      call Define.Label(S#AutoAllocate,base+1,54,0,'Out');      call Define.Label(S#AutoAllocate,base+2,54,0,'Dur');      call Define.Label(S#AutoAllocate,base+3,54,0,'Ofs');      Cue.Menu.Type = Define.Label(S#AutoAllocate,base,73,7,'');      Cue.Menu = Define.Table(S#AutoAllocate,1,4,Cue.Menu.Type);      call Fill.String.Table(Cue.Menu,0,0,O#TABL.Fill.Down,' EDIT| PLAY| TAKE| ERASE');      Drive.Type  = Define.Label(S#AutoAllocate,base+1,5,3,'');      Drive.Table = Define.Table(S#AutoAllocate,16,1,Drive.Type);      call Fill.String.Table(Drive.Table,0,0,O#TABL.Fill.Across,' 01| 02| 03| 04| 05| 06| 07| 08| 09| 10| 11| 12| 13| 14| 15| 16');      Drive.Parm.Type = Define.Fixed.Point(S#Autoallocate,base+2,5,3,0,cf#right\cf#spfill);      Drive.Parm.Table= Define.Table(S#AutoAllocate,16,2,Drive.Parm.Type);   call Set.Default.Attr(0,ATTR#Normal);   Cue.Name     = Define.String(S#AutoAllocate,base,5,48,0);   Cue.In.Time  = Define.32Bit.MSEC(S#AutoAllocate,base  ,58,14,O#MSEC.SMPTE,cf#default,cf#default);   Cue.Out.Time = Define.32Bit.MSEC(S#AutoAllocate,base+1,58,14,O#MSEC.SMPTE,cf#default,cf#default);   Cue.Dur.Time = Define.32Bit.MSEC(S#AutoAllocate,base+2,58,14,O#MSEC.SMPTE,cf#default,cf#default);   Cue.Mrk.Time = Define.32Bit.MSEC(S#AutoAllocate,base+3,58,14,O#MSEC.SMPTE,cf#default,cf#default);end Open.Cue.Window;call Draw.Track.Template(4,track.master);call Set.Term.attr(ATTR#Normal);call Fill.TRD.Area(4);call Update.Track.Display(0,0,11);