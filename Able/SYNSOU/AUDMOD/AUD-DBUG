debug.text:proc(xyz);   dcl xyz array;end;DEBUG: proc(str);   dcl str array;   dcl count fixed static;   dcl i fixed;   call pc(27); call ps('[20;24r');   call cpos(23,0);   call pc(a.lf);call pc(a.cr);   call ps('['); call pnum(count,0); call ps(']');   call ps(str);   call pc(sp);   if (screen.event.group = Panel.Menu.Group)  then call ps('Panel.Menu.Group');   /*   else if (screen.event.group = Project.Manager.Group)  then call ps('Project.Manager.Group');   else if (screen.event.group = Shuttle.Group) then call ps('Shuttle.Group');   else if (screen.event.group = Cue.Group) then call ps('Cue.Group');   else if (screen.event.group = Cue.Menu.Group) then call ps('Cue.Menu.Group');   else if (screen.event.group = Record.Group) then call ps('Record.Group');   else if (screen.event.group = Sync.Group) then call ps('Sync.Group');   else if (screen.event.group = Routing.Group) then call ps('Routing.Group');   else if (screen.event.group = Storage.Group) then call ps('Storage.Group');   */   else if (screen.event.group = Seq.Group) then call ps('Seq.Group');   else if (screen.event.group = Seq.Save.Group)    then call ps('Seq.Save.Group');   else do;      call ps('Group: ');       call pnum(screen.event.group,0);   end;      call pc(sp);   if      (screen.event.code = get.ertyp)        then call ps('get.ertyp');   else if (screen.event.code = get.new.seq.info) then call ps('get.new.seq.info');   else if (screen.event.code = get.screen.event) then call ps('get.screen.event');   else if (screen.event.code = get.play.scrolling) then call ps('get.play.scrolling');   else if (screen.event.code = get.program.exit) then call ps('get.program.exit');   else if (screen.event.code = get.DTD.info) then call ps('get.DTD.info');   else do;      call ps(' Code: ');       call pnum(screen.event.code,0);   end;   call pc(sp);   if      (screen.event.info = S#Deposit)        then call ps('S#Deposit');   else if (screen.event.info = S#MouseSelect)   then call ps('S#MouseSelect');   else if (screen.event.info = S#MouseActivate) then call ps('S#MouseActivate');   else if (screen.event.info = S#KeyActivate)   then call ps('S#KeyActivate');   else do;      call ps(' Info: ');       call pnum(screen.event.info,0);   end;   call pc(sp);   if      (Screen.Event.Id = Save.Button)          then call ps('Save.Button');   else if (Screen.Event.Id = New.Name.Button) then call ps('New.Name.Button');   else if (Screen.Event.Id = Replace.Button) then call ps('Replace.Button');   else if (Screen.Event.Id = Sequence.Name) then call ps('Sequence.Name');   else if (Screen.Event.Id = Cancel.Button) then call ps('Cancel.Button');   else if (Screen.Event.Id = Corner.Label) then call ps('Corner.Label');   else do;      call ps(' ID:');      call pnum(screen.event.id,0);    end;   call pc(sp);   call pc(27); call ps('[K');   call pc(27); call ps('[1;24r');   count=count+1;end DEBUG;eof