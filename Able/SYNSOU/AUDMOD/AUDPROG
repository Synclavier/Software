/* AUDMOD - Audio Event Editor Module */insert ':synlits:comlits';  /* Compilation literals */insert ':synlits:globlits';insert ':synlits:asciilit';insert ':synmods:globdcl';  insert ':synmods:getdcl';insert ':synmods:getvdcl';insert ':synmods:ttydcl';   insert ':synmods:plotdcl';  insert ':synmods:scrndcl';insert ':synmods:mousedcl'; insert ':synmods:smgrdcl';  insert ':synmods:errdcl';insert ':synmods:syndumm1'; insert ':synmods:loddcl';insert ':synmods:tprimdcl';insert ':synmods:mathdcl';insert ':synmods:tformdcl';insert ':synmods:xmemdcl';insert ':-xpl:catROUT';LIBRARY ':-XPL:LIBS:CATSWAP';PDL 2000;BEGIN;   RUN.SYN.LOOP:  PROC   PUBLIC;       dcl (save.mam,save.mal) fixed;      save.mam = read("60");      save.mal = read("61");      call main.loop;              /* this does the lod scan */      write("60") = save.mam;      write("61") = save.mal;   END;    END;insert 'AUD-MAIN';call Mouse.Init;call AUDIO.EVENT.EDITOR.SCREEN(0);