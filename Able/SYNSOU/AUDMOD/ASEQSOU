/* :SYNSOU:AUDMOD:ASEQSOU  $TITLE  Audio Event Editor ROUTING MODULE source *//*Modified:1998/05/20 - TY  - Added external references to Click Track routine & global variables.1998/05/07 - TY  - Added external references to Tempo/Meter Map routines.1990/01/25 - MWH - Fix "300 msec delay starting cues" bug (from EditView)1988/06/23 - MWH - Remove unused inserts*/MODULE ASEQSOU;insert ':-xpl:syslits';insert ':synlits:comlits';	/* Compilation literals */insert ':synlits:globlits';insert ':synlits:asciilit';insert ':synlits:lpnlits';insert ':synlits:synclits';insert ':synlits:thdlits';insert ':synlits:timlits';insert ':synlits:samplits';insert ':synlits:prmlits';insert ':synauxs:polylits';insert ':synmods:globdcl';  insert ':synmods:getdcl';insert ':synmods:getvdcl';insert ':synmods:loddcl';insert ':synmods:seqdcl';insert ':synmods:xmemdcl';insert ':synmods:ttydcl';insert ':synmods:plotdcl';insert ':synmods:tformdcl';insert ':synmods:mathdcl';insert ':synmods:smgrdcl';insert ':synmods:mousedcl';insert ':synmods:giddcl';insert ':synmods:d40dcl';dcl RECD.TRK        fixed external;dcl TBUT.PTR        fixed external;dcl TIM.PTR         fixed external;dcl TRK.HEAD.LOOKUP    PROC(FIXED,FIXED) EXTERNAL;       /* LOOK UP SINGLE WORD FROM SPECIFIED TRACK HEADER AREA */dcl TIM.HEAD.LOOKUP    PROC(FIXED,FIXED) EXTERNAL;       /* LOOK UP SINGLE WORD FROM SPECIFIED TRACK HEADER AREA */dcl TRK.HEAD.STORE     PROC(FIXED,FIXED,FIXED) EXTERNAL; /* STORE SINGLE WORD INTO SPECIFIED TRACK HEADER AREA */dcl LOOKUP.TRACK.TIMBRE.NAME PROC(FIXED,ARRAY) EXTERNAL; /* LOOKS UP CURRENT TIMBRE NAME FOR A TRACK */dcl TRK.HEAD        fixed external;dcl M$PRESSES       fixed external;DCL M$RELEASES      fixed external;DCL (#TRK)            FIXED EXTERNAL; /* ABS TRK NUMBER ACTION IS TO TAKE PLACE ON */DCL (#PTR)            FIXED EXTERNAL; /* ABS PTR TO TRACK HEAD */DCL (#NMSB,#NLSB)     FIXED EXTERNAL; /* NEXT TIME CORRESPONDING TO PTR */DCL (#LMSB,#LLSB)     FIXED EXTERNAL; /* LAST TIME CORRESPONDING TO PTR */DCL (#WRD,#SEC)       FIXED EXTERNAL; /* RELATIVE PTR INTO NAH AREA AT WHICH ACTION OCCURS */DCL (#ANYF,#ANYR)     FIXED EXTERNAL; /* ANY REV & FOR FOR THIS POINT   */DCL (#W1,#W2,#W3,#W4) FIXED EXTERNAL; /* 4 WORDS TO HOLD A NOTE RECORD */DCL (#MSB,#LSB)       FIXED EXTERNAL; /* GENERAL 32-BIT VALUE TO PASS OR RETURN */DCL SFILE.BASE        ARRAY EXTERNAL; /* RETURNS SECTOR OF POLY/EXT MEM IN GLOBAL VARIABLES */dcl STOP.SEQUENCER    proc  external;DCL CONTINUE.SEQUENCER        PROC EXTERNAL;DCL CONTINUE.SEQ.WITH.PRE.ROLL PROC (FIXED) EXTERNAL;dcl ISA.ILP.NOTE      proc  external;DCL ISA.REST.NOTE     PROC  EXTERNAL;DCL ISA.RTE.NOTE      PROC  EXTERNAL;dcl TBUT.MAPPING      proc (fixed) external;dcl TROUT.TRK#        fixed        external;dcl SELECT.PARAMETER  proc (fixed) external;dcl ALLOCATE.TRK.HEADER proc (fixed) external;dcl COPY.TIMBRE         proc (fixed,fixed) external;dcl NEW.MOTION             fixed           external;dcl NEW.CUE.TRACK.SETTINGS fixed           external;dcl MULTI.MAX              fixed           external;DCL TOGGLE.TRACK.SOLO.STATE PROC (FIXED) EXTERNAL; /* TOGGLE SOLO STATE OF TRACK OR GROUP */DCL CLEAR.TRACK.SOLO.STATES PROC         EXTERNAL; /* CLEAR ALL SOLOS					  */dcl Process.Erase.Button     proc external;dcl Display.Sequencer.Status proc external;dcl Num.Of.Soloed.Tracks     fixed external;dcl ERAS                     fixed external;dcl CLEAR.ERAS.BUTTON		 proc  external;dcl ERASE.STYLE				 fixed external;dcl TAKE.SYSTEM.SEQUENCE.TIME proc(array) external;dcl STOP.INFO.NOTE            proc(fixed) external;dcl Bounce.Down               proc(fixed,fixed,fixed,fixed) external;dcl GARBAGE.COLLECT.TRK       proc(fixed)             external;dcl REMOVE.ILP.FROM.TRACK     proc(fixed)             external;dcl CREATE.ILP.ON.TRACK       proc(fixed,fixed,fixed,fixed,fixed) external;dcl REMOVE.NOTE.RECORD        proc                    external;dcl PLAY.INITIALIZE           PROC                    external;DCL ADD.CUE.TO.LIST      PROC(FIXED,ARRAY,FIXED,FIXED) EXTERNAL;DCL LOAD.SEQ.GLOBALS          PROC(FIXED)         EXTERNAL;DCL INSERT.NOTE.RECORD    PROC (FIXED,FIXED)       RETURNS(FIXED) EXTERNAL; /* INSERT NOTE RECORD */DCL BACKUP.SEQUENCER.SUBROUTINE PROC (FIXED,FIXED,FIXED)  EXTERNAL; /* REWINDS SEQUENCER IN NON-REAL TIME */dcl P.LOOKUP              proc (fixed) external;dcl play.time.msb           fixed external;dcl Play.time.lsb         fixed external;dcl Samp.Click.Rate       fixed external;dcl Samp.BPM              fixed external;dcl ADVANCE.TO.NEXT.NOTE  proc  external;dcl PERFORM               proc  external;dcl CLICK.TRACK.MODE      fixed external;/* Tempo/Meter Map Routines: */dcl Map.Sequence.Time.To.Real.Time         proc(array, array)        external;dcl Map.Sequence.Duration.To.Real.Duration proc(array, array, array) external;dcl Map.Real.Time.To.Sequence.Time         proc(array, array)        external;dcl Map.Real.Duration.To.Sequence.Duration proc(array, array, array) external;dcl Map.Measures.Beats.To.Sequence.Time    proc(array, array)        external;      dcl Map.Sequence.Time.To.Measures.Beats    proc(array, array)        external;dcl Lookup.Beats.Per.Measure               proc(array) returns (fixed) external;/*********** EXTERNAL DECLARATIONS FOR '131-MAP' ***********/DCL REMAPPED.TIME.MSB      FIXED EXTERNAL;DCL REMAPPED.TIME.LSB      FIXED EXTERNAL;DCL REMAPPED.DUR.MSB       FIXED EXTERNAL;DCL REMAPPED.DUR.LSB       FIXED EXTERNAL;DCL REMAP.WITH.LIVE.CLICK  PROC (FIXED,FIXED,FIXED,FIXED,FIXED) EXTERNAL; /* CONVERTS TIMES USING LIVE CLICK TRACKS */DCL DRAW.MESSAGE          proc (array) external;DCL ERASE.MESSAGE         proc external;dcl format.error            proc external;dcl nomem.error             proc external;dcl no.changes.allowed      proc external; /* changes not allowed             */dcl term.input.err          proc external; /* input not recognized            */dcl feature.not.available   proc external; /* feature is not available        */dcl no.edit.allowed         proc external; /* no note to edit                 */dcl no.add.delete.allowed   proc external; /* cannot add or delete notes here */dcl cannot.change.pitch     proc external; /* cannot set pitch to desired value */dcl CANT.EDIT.LOOP          proc external;dcl PS.ERROR                proc (fixed) external;dcl PRESCAN                 fixed external;DCL NEW.SEQ.MATCH.W1 FIXED external;DCL NEW.SEQ.MATCH.W2 FIXED external;DCL NEW.SEQ.MATCH.W3 FIXED external;DCL NEW.SEQ.MATCH.W4 FIXED external;DCL RTMSB            FIXED EXTERNAL;DCL RTLSB            FIXED EXTERNAL;begin;insert ':synmods:audmods:aglobdcl';insert ':synmods:audmods:anotedcl';insert ':synmods:audmods:aicondcl';insert ':synmods:audmods:apmendcl';insert ':synmods:audmods:amotdcl';insert ':synmods:audmods:asyncdcl';insert ':synmods:audmods:acuemdcl';insert ':synmods:audmods:adtddcl';dcl Shuttle.Group                fixed                    external;dcl RECALL.CURRENT.CUE.BY.ID     proc (fixed,fixed,array) external;insert ':synauxs:audauxs:aseqlits';insert ':synsou:audmod:aud-seq';insert ':synsou:audmod:aud-seq0';insert ':synsou:audmod:aud-seq1';insert ':synsou:audmod:aud-seq2';insert ':synsou:audmod:aud-seq3';insert ':synsou:audmod:aud-seq4';insert ':synsou:audmod:aud-seq5';insert ':synsou:audmod:aud-seq6';insert ':synsou:audmod:aud-seq7';end;end ASEQSOU;