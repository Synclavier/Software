/* AUD-CUE6   $TITLE  Cue Edit Scroll Bar literals */dcl Fine.Bar.Play.Code    lit '(1)';/* Static variables */dcl AEE.Memory.Array (15)   fixed public;dcl AEE.Memory.Start (15)   fixed public;dcl AEE.Memory.Format( 7)   fixed public;dcl Shuttle.Percentage      fixed;/* sets the highlighting of the sbar table to match the        *//* passed bits                                                 */Highlight.AEE.Shuttle.Sbar.Control.Table:proc (Id) swapable;   dcl id      fixed;   dcl bitinfo fixed;   bitinfo = bits(GID(Saved.AEE.Shuttle.Coarse.Switch) + Coarse.Switch.Default)           \ bits(GID(Saved.AEE.Shuttle.Fine.Switch)   + Fine.Switch.Default + 3);   call Highlight.Graphics.Button.Table (Id, bitinfo, 2, 0);end Highlight.AEE.Shuttle.Sbar.Control.Table;