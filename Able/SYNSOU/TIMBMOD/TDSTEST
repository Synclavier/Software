/* TDSTEST - $Title Test Program for Numeric Timbre Display */insert ':synlits:comlits';          /* pdl size, configuration       */insert ':synmods:syncldcl';         /* bind in real time code        */insert ':synmods:mousedcl';         /* test program mouse routine    */insert ':synmods:ttydcl';           /* for ps                        */insert ':synmods:scrndcl';          /* for erase.rest.of.line        */insert ':synmods:tdsndcl';          /* get numeric timbre display screens  */insert ':synmods:tdsgdcl';          /* get graphic screens */call ps ('Timbre Display Screen Test ');call pcr;dcl i fixed;call initialization;                /* real time initialization      */call mouse.init;                    /* mouse     initialization      */do while 1;   call erase.rest.of.line(23,0);   call cpos(23,0);   call ps('Enter Test Code --');   call tty_wait;   input i;   do case i;      call TDS.GRAPHIC.DISPLAY(0);      call TDS.NUMERIC.DISPLAY(0);      call TDS.PARTIAL.DISPLAY(0);      call TDS.TIMBRE.FRAME.DISPLAY(0);     /* call to activate timbre frame screen */      call TDS.3D.DISPLAY(0);      ;   END;end;