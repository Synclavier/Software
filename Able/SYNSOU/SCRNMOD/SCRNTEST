INSERT ':SYNLITS:COMLITS';INSERT ':SYNMODS:SCRNDCL';insert ':synmods:ttydcl';insert ':synmods:syndummy';   call PRINT.SCREEN.FRAME(3,'TIMBRE DIRECTORY');   call cpos(1,2);   call ps('1. Use arrows to move cursor, <RETURN> to call up timbre, ');   if vt100<>0 then call ps('<ENTER>'); else call ps('<ESC><ESC>');   call ps(' when done.');   call cpos(2,2);   call ps('2. Select devices with 1,2,3,....');   call cpos(3,2);   call ps('3. Devices-->');   call tty_wait;DCL (I,J,K,L,M) FIXED;DO WHILE 1;   CALL CPOS(12,40);   call tty_wait;   INPUT I,J,K,L;   CALL DRAW.BOX(I,J,K,L);   CALL CPOS(12,40);   call tty_wait;   INPUT M;   CALL ERASE.BOX(I,J,K,L);END;