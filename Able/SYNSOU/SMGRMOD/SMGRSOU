/* Screen Manager Module */module smgrmod;insert ':SYNLITS:COMLITS';    /* BLACK MAGIC */insert ':SYNLITS:GLOBLITS';   /* ???         */insert ':SYNLITS:ASCIILIT';   /* A,B,C,D,E,F,.... */insert ':SYNAUXS:SMGRLITS';   /* FIND OUT ABOUT OURSELVES */insert ':SYNAUXS:ERRLITS';    /* ERROR MESSAGE LITERALS   */insert ':SYNLITS:THDLITS';insert ':SYNMODS:GLOBDCL';    /* GROOVY UNIVERSALS */insert ':SYNMODS:TTYDCL';     /* TERMINAL PRIMITIVES */insert ':SYNMODS:PLOTDCL';    /* PLOTTING PRIMITIVES */insert ':SYNMODS:SCRNDCL';    /* TERM HIGH BUT LOW LEVEL PRIMITIVES */insert ':SYNMODS:MATHDCL';    /* NUMERICAL GYMNASTICS FOR SMPTE,32-BITS,ETC. */insert ':SYNMODS:TFORMDCL';   /* TEXT FORMATTING ROUTINES */insert ':SYNMODS:GETDCL';     /* GET NEXT EVENT MODULE */insert ':SYNMODS:GETVDCL';    /* GET NEXT EVENT VARIABLES */insert ':SYNMODS:MOUSSDCL';   /* MOUSE USER FUNCTIONS--NOT REAL TIME */insert ':SYNMODS:MOUSRDCL';   /* MOUSE REAL TIME */insert ':SYNMODS:XMEMDCL';    /* COPY EXT MEM    */insert ':SYNSOU:SMGRMOD:SMG-SMAN';end smgrmod;