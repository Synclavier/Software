/* STORSOU  $TITLE  Sound File Directory Screen */module STORMOD;insert ':synlits:comlits';    /* get configuration and pdl size */insert ':synlits:globlits';insert ':synmods:globdcl';         insert ':synmods:tprimdcl';insert ':synmods:stor1dcl'; /* this is where the real code sits */RECALL.SOUND.FILE.SCREEN:proc (par#,fra#,immed) public swapable;   dcl (par#,fra#,immed) fixed;   call RECALL.SOUND.FILE.SCREEN.DRIVER(par#,fra#,immed);   return Goto.This.Screen;   /* return state info */end RECALL.SOUND.FILE.SCREEN;end STORMOD;