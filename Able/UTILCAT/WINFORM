/* Winchester formatting insert file */library ':utilcat:libs:winform';/* Statuses returned by SCSIFORMAT (except SCSICONNECT statuses) */dcl GoodFormat    lit '0';               /* Returned by SCSIFORMAT if format was successful */dcl DeviceFailed  lit '-6';              /* Returned by SCSIFORMAT if SCSI controller/device failed */dcl FormatAborted lit '-7';              /* Returned by SCSIFORMAT if user aborts format operation */dcl winform    proc(fixed, fixed, fixed, fixed array) returns (fixed) external; /* format the winchester */