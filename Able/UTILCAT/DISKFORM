/* Floppy formatting insert file */library ':utilcat:libs:diskform';dcl set.up    proc(fixed) external; /* set up floppy variables */dcl disk.form proc(fixed, fixed array) returns(fixed) external; /* call to format diskette as indicated by config table */dcl test      proc external;dcl cpmflag boolean external; /* true if this is a CP/M format; false if XPL format */