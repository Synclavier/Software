write("50") = "15";write("50") = "12";write("300") = read("300");if (read("302") < 0)then write("50") = "61";else write("50") = "60";write("300") = read("300");write("50") = shr(read("302"),12) + "60";write("300") = read("300");write("50") = shr(read("302"),9) + "60";write("300") = read("300");write("50") = shr(read("302"),6) + "60";write("300") = read("300");write("50") = shr(read("302"),3) + "60";write("300") = read("300");write("50") = read("302") & "7" + "60";