dcl pi                 lit '3.14159265';dcl degrees_to_radians lit '(pi/180)'; /* radians/degree */asn: proc(x) returns (floating); /* arcsine [defined from -pi/2 to pi/2] */   dcl x floating;   if x = 1.0 /* arcsine of 1? (function is undefined here) */   then return (pi/2.0); /* yes, result is pi/2 radians (90 degrees) */   else return (atn (x/sqr(1 - x*x))); /* no, calculate it */end asn;acs: proc(x) returns (floating); /* arccosine [defined from 0 to pi] */   dcl x floating;   if x = 0.0 /* arccosine of 0? (function is undefined here) */   then return (0.0); /* yes, result is 0 radians (0 degrees) */   else return (atn (sqr(1 - x*x)/x)); /* no, calculate it */end acs;dcl (a, b) floating;a = 100.0;b = 10.0;print a, ' ', b;a = a + b; b = b - 15.0; /* add/sub */print a, ' ', b;b = b + 15.0; a = a - b; /* add/sub */print a, ' ', b;a = a*b; b = b*15.0; /* multiply */print a, ' ', b;b = b/15.0; a = a/b; /* divide */print a, ' ', b;a = sqr(a); b = -b; /* square root/sub */print a, ' ', b;a = a*a; b = abs(b); /* multiply/abs */print a, ' ', b;a = log(a); b = tan(b*degrees_to_radians); /* log/tan(in radians) */print a, ' ', b;a = int(exp(a) + .5); b = int(atn(b)/degrees_to_radians + .5); /* exp/atn(in degrees) */print a, ' ', b;a = sin(a*degrees_to_radians); b = cos(b*degrees_to_radians); /* sin(in radians)/cos(in radians) */print a, ' ', b;a = int(asn(a)/degrees_to_radians + .5); b = int(acs(b)/degrees_to_radians + .5); /* asn(in degrees)/acs(in degrees) [NOTE asn(sin(100 deg.) = 80 deg.!!] */print a, ' ', b;