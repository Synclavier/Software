MODULE SOFT;   CONFIGURATION NOMULDIV;   SM:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I*J;   END SM;   SFM:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I%J;   END SFM;   SD:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I/J;   END SD;   SFD:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I FDIV J;   END SFD;END SOFT;