dcl a data ('ABCDEFGHIJKLMNOPQRSTUVWXYZ');dcl (i, j) fixed;print string (a); /* use string */do i = 0 to a (0) - 1; /* now use byte */   print character(byte(a, i)),;end;print;do i = 0 to a (0)/2 - 1; /* reverse string with byte and pbyte */   j = byte(a, a (0) - i - 1);   call pbyte (a, a (0) - i - 1, byte(a, i));   call pbyte (a, i, j);end;print string (a); /* and print it */