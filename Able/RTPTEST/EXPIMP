dcl a (29) fixed;dcl b (29) fixed;dcl i fixed;write("60")=2; write("61")=251;do i = 0 to 29;   a (i) = i;   b (i) = -1;end;call export (0, 0, a, 30);call import (0, 0, b, 30);if i <> 30 then print 'I was zapped by IMPORT', i;do i = 0 to 29;   if a (i) <> i then print 'A has been changed', i;   if a (i) <> b (i) then print 'A <> B', i, b (i);end;call export (0, 257, a, 30);call import (1, 1, b, 30);if i <> 30 then print 'I was zapped by IMPORT', i;do i = 0 to 29;   if a (i) <> i then print 'A has been changed', i;   if a (i) <> b (i) then print 'A <> B', i, b (i);end;call extset (0, 0, 30, 0);call import (0, 0, a, 30);do i = 0 to 29;   if a (i) <> 0 then print 'A not zeroed', i, a (i);end;call extset (0, 0, 30, 1);call import (0, 0, a, 30);do i = 0 to 29;   if a (i) <> 1 then print 'A not set to one', i, a (i);end;if b (0) <> 0 then print 'B (0) was zapped by IMPORT', b (0);call extset (0, 257, 30, 0);call import (1, 1, a, 30);do i = 0 to 29;   if a (i) <> 0 then print 'A not zeroed', i, a (i);end;if read("60") <> 2 then print 'D60 was not preserved!';if read("61") <> 251 then print 'D61 was not preserved!';