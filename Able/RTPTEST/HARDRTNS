MODULE HARD;   CONFIGURATION MULDIV;   HM:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I*J;   END HM;   HFM:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I%J;   END HFM;   HD:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I/J;   END HD;   HFD:PROC(I,J) RETURNS(FIXED) PUBLIC;      DCL (I,J) FIXED;      RETURN I FDIV J;   END HFD;END HARD;