/* WARNING:  This test may wipe out your Winchester!  Run DISKTEST first. */dcl buf  (255) fixed;dcl buf2 (255) fixed;dcl i          fixed;call readdata (0, 4, buf, 256);call writedata (0, 4, buf, 256);call diskread (4, buf2, 256);do i = 0 to 255;   if buf (i) <> buf2 (i) then do;      print 'WRITEDATA didn''t work.';      call exit(-1);   end;end;do i = 0 to 255; /* write out all -1's this time */   buf2 (i) = -1;end;call writedata (0, 4, buf2, 256);do i = 0 to 255; buf2 (i) = 0; end; /* be paranoid about READDATA */call readdata (0, 4, buf2, 256);do i = 0 to 255;   if buf2 (i) <> -1 then do;      print 'WRITEDATA failed the -1 test.';      call writedata (0, 4, buf, 256); /* try to put original sector back (WRITEDATA worked above) */      call exit(-1);   end;end;call diskwrite (4, buf2, 256);do i = 0 to 255; buf2 (i) = 0; end; /* be paranoid about DISKREAD */call diskread (4, buf2, 256);do i = 0 to 255;   if buf2 (i) <> -1 then do;      print 'DISKWRITE failed the -1 test.';      call writedata (0, 4, buf, 256); /* try to put original sector back (WRITEDATA worked above) */      call exit(-1);   end;end;call diskwrite (4, buf, 256); /* restore the sector */call diskread (4, buf2, 256);do i = 0 to 255;   if buf (i) <> buf2 (i) then do;      print 'DISKWRITE didn''t work.';      call writedata (0, 4, buf, 256); /* try to put original sector back (WRITEDATA worked above) */      call exit(-1);   end;end;