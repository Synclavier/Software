NE.FRAME.ICON				proc(fixed, fixed, fixed)		external;dcl TURN.ON.INFO.TRKS				proc									external;dcl TURN.ON.ENABLE.TRKS				proc									external;dcl SET.SOLOED.TRK.BITS				proc(array)							external;dcl Define.Labeled.32Bit			proc(fixed, fixed, array, fixed, fixed, fixed, fixed, fixed, fixed)	external;dcl Define.Labeled.Fixed.Point	proc(fixed, fixed, array, fixed, fixed, fixed, fixed)	external;dcl Define.Labeled.Switch			proc(fixed, fixed, array, fixed, array, fixed)	external;dcl DISPLAY.AND.UNDEFINE.OBJ		proc(fixed)							external;dcl HANDLE.SEQ.EDIT.OBJ.DRAG		proc									external;dcl DEFINE.TIMES.GROUP				PROC									external;dcl DEFINE.TRK.MGR.DISP				PROC									external;dcl set.track.enable.state			proc(fixed,fixed,fixed)			external;dcl UPDATE.CLICK.SWITCHES			proc									external;dcl OPEN.TRK.MGR.DISP				PROC									external;dcl GET.TIME.EVENT					proc(fixed) returns (fixed)	external;dcl GET.TRK.MGR.EVENT				proc(fixed) returns (fixed)	external;dcl GET.NEXT.EDIT.SCREEN.EVENT	proc									external;//	dcl GET.DEBUG.EVENT					proc(fixed) returns (fixed)	external;	//	<<<<<<<<	not used//	dcl DBUG									proc(array)							external;	//	<<<<<<<<	not usedlibrary ':syntlibs:editalib';