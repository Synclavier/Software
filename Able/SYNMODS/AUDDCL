/* AUDDCL --  Audio Event Editor */dcl AUDIO.EVENT.EDITOR.SCREEN  proc (fixed) external;/* Audmod brings in the DTD screens as well */dcl DIRECT.TO.DISK.SONG.DIRECTORY proc(fixed) external;  /* call to activate DTD song directory*/dcl DIRECT.TO.DISK.TRACK.DISPLAY  proc(fixed) external;  /* call to activate DTD track display*/library ':syntlibs:audlib';library ':synlibs:linklib';  /* needs link mod as well */library ':syntlibs:dtdlib';  /* direct-to-disk song directory and track display */