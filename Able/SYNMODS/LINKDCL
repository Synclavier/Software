/* LINKDCL -- Link Module */dcl UPDATE.CLICK.STATE.VARS				proc												external;//	dcl UPDATE.CLICK.DISP.FORMAT				proc												external;dcl UPDATE.SYNCLAV.CLICK.MODE				proc												external;dcl SET.SYNCLAV.CLICK.MODE					proc (fixed)									external;dcl GET.SYNCLAV.CLICK.MODE					proc						returns (fixed)	external;dcl SET.SYNCLAV.CLICK.DISP.FORMAT		proc (fixed)									external;dcl GET.SYNCLAV.CLICK.DISP.FORMAT		proc						returns (fixed)	external;dcl SET.SYNCLAV.CLICK.SOUND				proc (fixed)									external;dcl GET.SYNCLAV.CLICK.SOUND				proc						returns (fixed)	external;dcl SET.SYNCLAV.LIVE.CLICK.TRACK			proc (fixed)									external;dcl GET.SYNCLAV.LIVE.CLICK.TRACK			proc						returns (fixed)	external;dcl HANDLE.CLICK.MODE.SWITCH				proc (fixed)									external;dcl SET.SYNCLAV.CLICK.RATE					proc (array, fixed)							external;dcl UPDATE.CLICK.FIELD						proc (fixed, fixed)							external;dcl SET.SYNCLAV.SMPTE.MODE					proc (fixed)									external;dcl SET.SYNCLAV.SMPTE.OFFSET				proc (array)									external;dcl CONSTRUCT.SYNC.MODE.SWITCH.STRING	proc (array)									external;dcl SET.SYSTEM.SYNCLAV.SYNC.MODE			proc (fixed)									external;dcl SET.SYNCLAV.SYNC.MODE					proc (fixed)									external;dcl GET.SYNCLAV.SYNC.MODE					proc						returns (fixed)	external;dcl GET.FEET.FPS								proc						returns (fixed)	external;dcl SET.SYNCLAV.FEET.MODE					proc (fixed, fixed)							external;dcl SET.SYNCLAV.FEET.OFFSET				proc (array)									external;dcl PARSE.SYNCLAVIER.PARM					proc (fixed, fixed)	returns(fixed)		external;dcl TAKE.SYSTEM.REAL.TIME					proc (array)									external;dcl TAKE.SYSTEM.SEQUENCE.TIME				proc (array)									external;dcl TAKE.SYSTEM.SMPTE.TIME					proc (array)									external;library ':synlibs:linklib';