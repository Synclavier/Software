/*	:SYNMODS:GIDDCL  $TITLE  External Dcls for PID/GID Module */dcl PID					 proc (fixed, fixed)				external;	/*	write word of info to PID/GID area	*/dcl GID					 proc (fixed) returns(fixed)	external;	/*	get word of info from PID/GID area	*/dcl PID32				 proc (fixed, fixed array)		external;	/*	write 2 consecutive words of info to PID/GID area	*/dcl GID32				 proc (fixed, fixed array)		external;	/*	get 2 consecutive words of info from PID/GID area	*/dcl Init.PID.GID.Area proc									external;	/*	zeros out dynamic area below static area	*/insert ':synauxs:gidlits';library ':synlibs:globlib';	/*	library globmod first to get mem.siz and examount set up	*/library ':synlibs:gidlib';