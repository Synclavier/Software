rnal;  /* ptr to next good message byte to be read     */dcl p.in.prior               fixed external;dcl num.of.new.packets       fixed external;  /* number of unprocessed D50 packets in buffer */dcl protocol.out.ptr         fixed external;  /* ext mem buffer for protocol output stream */dcl p.out.wptr               fixed external;  /* ptr to next free loc in output buffer */dcl p.out.rptr               fixed external;  /* ptr to next byte to send */dcl hereis.printed           fixed external;  /* flag when here_is message gets printed */dcl allow.proto.msgs         fixed external;  /* set with bits to log protocol messages */dcl Send.Events.To.D50.Termulator   proc                external;dcl Flush.VK.Display.To.Termulators proc                external;dcl GET.NEXT.D50.PROTOCOL.PACKET    proc (fixed)        external;DCL CHECK.FOR.PROTOCOL.STOP.BUTTON  proc returns(fixed) external;DCL Get.NodeName                    proc (fixed,array)  external;DCL D115.Download                   proc (fixed)        external;dcl EMPTY.MIDI.BUF                  proc                external;dcl SEND.PROTOCOL.PACKET            proc (fixed,fixed)  returns(fixed) external;dcl FAST.SEND.PROTOCOL.PACKET       proc (fixed,fixed)  returns(fixed) external;dcl SEND.PROTOCOL.PACKET.WITH.RETRY proc (fixed,fixed,fixed) returns(fixed) external;dcl map.d115.node.id                proc (fixed)        external;dcl PROCESS.MIDI.BULK.PACKET		proc (fixed, fixed, fixed) external;insert  ':synauxs:prolits';library ':synlibs:prolib';