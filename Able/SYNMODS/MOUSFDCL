/* MOUSFDCL   $TITLE  GENERAL USER MOUSE MODULE   Modified:   01/11/89 - TSS - Added dcls to control Termulator PROTOCOL transmission   04/01/88 - TSS - Added support for MAC II mouse from Termulator   11/25/87 - MWH - Remove unused "d40q_in_system"   */DCL MOUSE.CURSOR.TYPE     FIXED EXTERNAL;  /* SEE 523-MOUS FOR DEFINITIONS */DCL MOUSE.REFRESH.RATE    FIXED EXTERNAL;  /* #MSEC BETWEEN CURSOR UPDATES; THIS IS NOT THE MOUSE SCAN RATE (ALWAYS 20MSEC) */DCL MOUSE.CURSOR.ON       FIXED EXTERNAL;  /* USER DEFINED: 1 MEANS MOUSE CURSOR ENABLED; 0 MEANS DISABLED *//* INTERNAL TO MOUSE ROUTINES */DCL M$CURSOR.SIZE         FIXED EXTERNAL;  /* LENGTH OF EACH ARM OF MOUSE CURSOR CROSSHAIR */DCL MOUSE.HOLE.LOWERX     FIXED EXTERNAL;  /* LOWER LEFT CORNER OF MOUSE HOLE */DCL MOUSE.HOLE.LOWERY     FIXED EXTERNAL;DCL HIDE.TERMULATOR.CURSOR                  PROC EXTERNAL;DCL SHOW.TERMULATOR.CURSOR                  PROC EXTERNAL;DCL TERMULATOR.ABSOLUTE.MOUSE.REPORT.FORMAT PROC EXTERNAL;DCL TERMULATOR.RELATIVE.MOUSE.REPORT.FORMAT PROC EXTERNAL;DCL ENABLE.TERMULATOR.AUTO.MOUSE.REPORT     PROC (FIXED) EXTERNAL;DCL DISABLE.TERMULATOR.AUTO.MOUSE.REPORT    PROC EXTERNAL;DCL DISABLE.TERMULATOR.PROTOCOL             PROC EXTERNAL;DCL ENABLE.TERMULATOR.PROTOCOL              PROC EXTERNAL;DCL MOUSE.INIT           PROC EXTERNAL;DCL MOUSE                PROC (FIXED) RETURNS (FIXED) EXTERNAL;DCL MOUSE.SWITCH         PROC(FIXED) EXTERNAL;DCL PUSH.MOUSE           PROC EXTERNAL;DCL POP.MOUSE            PROC EXTERNAL;DCL CONVERT.MOUSE.X      PROC(FIXED) RETURNS(FIXED) EXTERNAL;DCL CONVERT.MOUSE.Y      PROC(FIXED) RETURNS(FIXED) EXTERNAL;DCL REBOOT.TERMINAL      PROC EXTERNAL;LIBRARY ':SYNLIBS:MOUSFLIB';