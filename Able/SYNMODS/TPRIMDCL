/* TPRIMDCL - PRIMITAVE ITEMS NEEDED BY ANY SCREEN */dcl  dir.device  fixed external;dcl beep.status  fixed external;dcl  DELETE.STR  data  external; dcl  RUBOUT      data  external;dcl  ENTER.STR   data  external; dcl  DBL.ESC     data  external;dcl  CURCAT.STR  data  external;dcl PRINT.EXIT.SCREEN.PROMPT   proc         external; /* displays message for how to leave screen */dcl Get.Device.Name            proc (fixed,array) external; /* compute string for name */dcl PRINT.CURRENT.CATNAME      proc (fixed) external; /* prints name and device of current cat */dcl PRINT.CCAT                 proc         external; /* positions cursor, prints current catalog */dcl PRINT.DIR.DEVICE.CATNAME   proc         external; /* print catname based on dir.device */dcl PRINT.CURRENT.TIMBRE.NAME  proc         external; /* draws current timbre name at bottom of screen */dcl PRINT.TIMBRE.NAME          proc(fixed,fixed)       external; /* prints current timbre in 1-1-1 "name" format at x,y */dcl DRAW.TIMBRE.NAME           proc(fixed,fixed,fixed) external; /* prints current timbre name in 1-1-1 "xyz" format in graphics mode - pass x,y,size */dcl ERASE.TIMBRE.NAME          proc(fixed,fixed,fixed) external; /* erases previous one - pass x,y,size */dcl DRAW.PARTIAL.NUMBER        proc(fixed,fixed,fixed,fixed) external; /* draws passed partial # at x,y,size */dcl ERASE.PARTIAL.NUMBER       proc(fixed,fixed,fixed) external;  /* erases previous one */dcl REMOVE.ERR                 proc         external; /* remove error message */dcl DISPLAY.ERROR.MESSAGE proc (fixed,fixed,fixed) external; /* display error at x,y */dcl Handle.System.Error.Codes  proc (fixed)        external; /* display 24 & 25 error messages */dcl EXIT.MY.SCREEN   fixed external;dcl GOTO.THIS.SCREEN fixed external;dcl RETURN.TO.PREV.STATE proc external;dcl MOUSE.HOLE.X      fixed external;dcl MOUSE.HOLE.Y      fixed external;dcl PLOT.MOUSE.HOLE proc(fixed,fixed) external;dcl MOUSE.HOLE      proc(fixed,fixed) external;DCL DRAW.MESSAGE          proc (array) external;DCL ERASE.MESSAGE         proc external;dcl Display_Update_Status proc(fixed,fixed,fixed) external; /* print status during laser disk update */insert  ':synauxs:tprimlit';   /* special lits */library ':synlibs:tprimlib';   /* code         */