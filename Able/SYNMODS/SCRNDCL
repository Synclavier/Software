/* SCRNDCL - $Title Primitive Terminal Routines Library and External definitions */dcl print_digits         proc(fixed,fixed) external; /* prints positive number in n places with leading zeros */dcl print.parm           proc(fixed,fixed,fixed,fixed,fixed) external; dcl DRAW.CORNER          proc(fixed) external; dcl DRAW.TEE             proc(fixed) external; dcl DRAW.HOR             proc(fixed) external; dcl DRAW.VER             proc(fixed,fixed,fixed) external;dcl DRAW.BOX             proc(fixed,fixed,fixed,fixed) external; dcl ERASE.BOX            proc(fixed,fixed,fixed,fixed) external; dcl DRAW.FRAME           proc(fixed, fixed array, fixed) external /* NON - SWAP */; dcl PRINT.BOX.WITH.TITLE proc(fixed,fixed,fixed,fixed,array) external;dcl ERASE.BOX.WITH.TITLE proc(fixed,fixed,fixed,fixed)       external;dcl PRINT.SCREEN.FRAME   proc(fixed, fixed array) external /* NON-SWAP */;insert ':synauxs:scrnlits';library ':synlibs:scrnlib';