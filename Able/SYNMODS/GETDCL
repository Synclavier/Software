/* GETDCL   $TITLE  Declarations for GETMOD   Modified:   06/27/89 - cj  - Moved many events around for release 2.2   01/17/89 - MWH - Send periodic time message in interrupt, not here   10/24/88 - MWH - Add loop escape when processing incoming D115 messages   */dcl GET_ENABLE              procedure(fixed) external;dcl GET_DISABLE             procedure(fixed) external;dcl ENABLE_SEVERAL_GETS     procedure(fixed,fixed,fixed,fixed) external;dcl DISABLE_SEVERAL_GETS    procedure(fixed,fixed,fixed,fixed) external;dcl ENABLE_BIT_GETS         proc (fixed) external;dcl DISABLE_BIT_GETS        proc (fixed) external;dcl ENABLE_ALL_GETS         procedure        external;dcl DISABLE_ALL_GETS        procedure        external;dcl Check.Next.Event 		proc  external;  		// Do housekeeping, main loop, performdcl get.item.enabled array external;dcl next.event.info  fixed external;  				/* info returned               */dcl last.event.code  fixed external;  				/* code returned               */dcl last.event.info  lit 'next.event.info'; 		/* also is the last event info */dcl get.next.event   proc  external;insert ':synauxs:getlits';library ':synlibs:getlib';