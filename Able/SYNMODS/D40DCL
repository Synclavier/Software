/* D40DCL - $Title D40 Interrupt Driven Printer External Declarations and Library */dcl enable_d40   proc         external; /* enables D40 interrupts  */dcl disable_d40  proc         external; /* disables D40 interrupts */dcl d40_wait     proc         external; /* wait for printer to finish printing */dcl send_string  proc (array) external; /* sends string to printer */dcl screen_dump  proc         external; /* prints copy of alpha chars on screen */dcl hardcopy     proc         external; /* dumps from the screen to the I.D.S.  */library ':synlibs:globlib';     /* library globmod first to get mem.siz and examount set up */library ':synlibs:d40lib';