/* :SYNMODS:SEQDCL  $TITLE  Sequencer Utility Programs *//*Modified:1998/05/16 - TY  - Split SCALE.RTE.VAL into SCALE.RTE.TO.PERCENT and SCALE.PERCENT.TO.RTE.1998/05/10 - TY  - Added SRSCALE dcl.1990/09/10 - TSS - Added SET.SEQUENCER.OUTPUT.ROUTING dcl*/dcl GET.PITCH.BEND.RANGE proc(fixed) returns (fixed) external;dcl EXTRACT.DUR          proc(array,array)   external;dcl EXTRACT.KEY          proc(array) returns(fixed) external;dcl BUILD.NOTE.RECORD    proc(array)         external; dcl SCROLL.SEQ           proc(fixed,fixed,fixed)   external;  /* only called when play=0 */dcl CHANGE.NOTE.START    proc (array,fixed)  external; /* changes start time of note in sequence */dcl CHANGE.NOTE.PITCH    proc (fixed)        external; /* changes pitch of note displayed under cursor */dcl CHANGE.NOTE.DURATION proc (array,fixed)  external; /* changes duration of note in sequencer */dcl SRSCALE					 proc (fixed,fixed,fixed) returns (fixed) external; /* SIGNED ROUNDING SCALE (value, numerator,denominator) */dcl SCALE.RTE.TO.PERCENT proc (fixed)			external;dcl SCALE.PERCENT.TO.RTE proc (fixed)			external;dcl CHANGE.RTE.VALUE     proc (fixed)        external;dcl CHANGE.RTE.CODE      proc (fixed)        external;dcl SET.SEQUENCER.OUTPUT.ROUTING proc (fixed,fixed,fixed) returns(fixed) external;dcl SCROLL.FORWARD.SOME.LINES   proc(fixed, fixed, array, fixed) external;dcl SCROLL.BACK.1.LINE          proc(array)                      external;dcl (trkindex)           array external;dcl #count               fixed external;dcl GET.SNAPSHOT.INFO proc (fixed,fixed,array,fixed) external; /* pass memptr, track#, trkindex, row# */dcl ERASE.SNAPSHOT.AREA proc (fixed,fixed)           external; /* pass memptr, #trks */dcl CURSOR.BEGIN      fixed array external,    CURSOR.END        fixed array external,    SCREEN.BEGIN      fixed array external,    SCREEN.END        fixed array external;dcl recorder.screen.snapshot proc (fixed, fixed, array,    /* xmem.ptr,#trks,trkindex,  */                                   fixed, fixed,           /* time.msb,time.lsb,        */                                   fixed, fixed) external; /* #rows,format           */dcl AEE.seq.screen.snapshot proc  (fixed, fixed, array,    /* xmem.ptr,#trks,trkindex,  */                                   fixed, fixed,           /* time.msb,time.lsb,        */                                   fixed, fixed) external; /* #rows,format           */dcl expand.event.note proc (array,fixed) external;  /* expands global variables #trk #w1 #w2 #w3 #w4 into expanded record info and name */DCL ALLOCATE.NLS.BLOCKS.FOR.DATA.STORAGE PROC (FIXED, FIXED) EXTERNAL; /* TRACK #, # OF BLOCKS */DCL COMPUTE.SOUND.FILE.OFFSET.TIME       proc (FIXED, FIXED, ARRAY, FIXED) EXTERNAL;dcl LOOK.UP.SOUND.FILE.CAPTION           proc (FIXED, FIXED, ARRAY, FIXED) EXTERNAL;dcl Get.SF.Name                          proc (FIXED, FIXED, ARRAY       ) EXTERNAL;dcl Compute.Default.Note                 proc (FIXED, ARRAY              ) EXTERNAL;dcl Audition.Music.Note                  proc                              EXTERNAL;insert ':synauxs:seqlits';insert ':synmods:eventdcl';library ':synlibs:seqlib';