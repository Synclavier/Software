/* TDSUDCL   $TITLE  Timbre Display Utility External Declarations */   dcl tds.change       fixed external; /* set to 1 if any screen updates take place */   dcl tds.choice       fixed external; /* screen selected */   dcl tds.last.choice  fixed external; /* last screen selected */   dcl previous.partial fixed external; /* string containing previous partial number */   dcl #ypos            fixed external; /* keep track of where mouse is on the screen for mapping purposes */   dcl DRAW.MENU.SELECTION     proc(fixed) external; /* draw the menu selection given the line number */   dcl POS.CHOICE.CURSOR       proc(fixed) external; /* position the menu selection cursor on the menu item*/   dcl MOUSE.SELECT.TDS.SCREEN proc(fixed) external; /* selects TDS screen with mouse */   dcl CHANGE.TDS.STATE        proc(fixed,fixed) external; /* changes to the given tds state */   dcl COMPARATIVE.SNAP.SHOT   proc external;        /* takes a picture of current timbre parms */   dcl EXTRACT.COMP.PARM       proc(fixed,fixed,fixed) returns (fixed) external; /* gets parameter from snap shot or display area */library ':syntlibs:tdsulib';  /* library the TDS utility routines */