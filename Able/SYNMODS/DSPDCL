/* DSPDCL  $TITLE  Insert file to access Time Scale Modification routines *//* Modified:   12/13/88 - cj  - changed 'smart' to 'speech' (and other changes) */dcl TSMData_There fixed external; /* true if TSM data file found at boot time */dcl TSMData_MS    fixed external; /* MS_Sector address of .TSMDATA file */dcl TSMData_LS    fixed external; /* LS_Sector address of .TSMDATA file */dcl TSMData_Words fixed external; /* Words of data in .TSMDATA file     */dcl Rescale.User.TSM.Ratio     proc (fixed)             external;dcl Compute.Compressed.Length  proc (array,array,fixed) external;dcl Compute.Expanded.Length    proc (array,array,fixed) external;dcl Compute.Ratio.For.Time.Fit proc (array,array,array) returns(fixed) external;dcl Select.DSP70               proc returns(boolean)    external; /* tries to select D24 #3 */dcl Time.Scale.Modify.Samples  proc (array,array,array,array,fixed,fixed,fixed) returns(fixed) external;dcl Modify.Samples             proc (array,array,array,array,fixed,fixed,array) returns (fixed) external;dcl Load.DSP70                 proc(array) external;dcl Edit.to.Symbol             proc(array) external;dcl Pitch.Detect               proc(array,array) external;dcl Peek.At.DSP70.Memory       proc(fixed,fixed,fixed) external;dcl SR.Convert                 proc(fixed) external;insert ':synauxs:dsplits';library ':synlibs:globlib';    /* put glob mod first so examount gets set */library ':synlibs:xmemlib';   library ':-xpl:libs:scsiswap'; /* scsi swap routines                      */library ':-xpl:libs:scsicmds';library ':-xpl:libs:catswap';  /* set catbuf here once                    */library ':synlibs:dsplib';