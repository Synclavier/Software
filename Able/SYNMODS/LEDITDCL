/* LEDITDCL -- LOD REEL EDIT DECLARATIONS  $TITLE  External Declarations for Reel Editing Module */dcl E$Status                         fixed external; /* edit operation status word */dcl CUE.LOOKUP                       proc (fixed,fixed)       returns (fixed) external;dcl CUE.STORE                        proc (fixed,fixed,fixed) returns (fixed) external;dcl READ.CUE.TIME                    proc (fixed,fixed,array) external;dcl WRITE.CUE.TIME                   proc (fixed,fixed,array) external;dcl COMPUTE.CUE.SEGMENT.PTR          proc (fixed,fixed)       returns (fixed) external;dcl PUT.EDIT.SEGMENT                 proc (fixed,fixed,array) external;dcl GET.EDIT.SEGMENT                 proc (fixed,fixed,array) external;dcl APPEND.EDIT.SEGMENT              proc (fixed,array)       returns (fixed) external;dcl SHUFFLE.EDIT.SEGMENTS            proc (fixed,fixed,fixed,fixed) external;dcl INITIALIZE.REEL                  proc (fixed)             external; /* turns old format "simple" cue into reel cue */dcl COPY.CUE                         proc (fixed,fixed,fixed) returns (fixed) external;dcl COPY.CUE.HEADER                  proc (fixed,fixed,fixed) returns (fixed) external;dcl GET.SEGMENT.DURATION             proc (array,array)       external; /* compute duration of passed edit array */dcl GET.SEGMENT.SYNC.END.TIME        proc (array,array)       external; /* compute sync end time of passed edit array */dcl GARBAGE.COLLECT.REEL             proc (fixed)             external;dcl CLEAR.EDITS.FROM.REEL            proc (fixed,fixed)       external;dcl EMPTY.REEL                       proc (fixed)             external;dcl DELETE.FROM.REEL                 proc (fixed,fixed,fixed) returns (fixed) external;dcl INSERT.LEADER.ON.REEL            proc (fixed,fixed,fixed) returns (fixed) external;dcl EXTRACT.FROM.REEL.WITH.EXTEND    proc (fixed,fixed,fixed) returns (fixed) external;dcl EXTRACT.FROM.REEL.WITHOUT.EXTEND proc (fixed,fixed,fixed) returns (fixed) external;dcl FILL.REEL.WITH.LEADER            proc (fixed,fixed,fixed) returns (fixed) external;dcl CUT.FROM.REEL                    proc (fixed,fixed,fixed,fixed,fixed) returns (fixed) external;dcl COPY.FROM.REEL                   proc (fixed,fixed,fixed) returns (fixed) external;dcl PASTE.INTO.REEL                  proc (fixed,fixed,fixed,fixed,fixed) returns (fixed) external;dcl CHAIN.ONTO.REEL                  proc (fixed,fixed,fixed,fixed) returns (fixed) external;dcl FILL.REEL                        proc (fixed,fixed,fixed,fixed,fixed) returns (fixed) external;dcl SLIDE.EDIT.REGION                proc (array,array,array,fixed,fixed,fixed,fixed) returns (fixed) external;dcl MIX.REELS                        proc (fixed,fixed,fixed,fixed) returns (fixed) external;dcl EXCHANGE.REEL.WITH.EDIT.REGION   proc (fixed,fixed,fixed,fixed) returns (fixed) external;insert  ':synauxs:leditlit';   /* edit segment literals              */library ':synlibs:lodlib';     /* code is linked in with lod for now */