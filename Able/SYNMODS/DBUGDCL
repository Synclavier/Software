/* DBUGDCL -- debug screen *//*   7/28/86 - TS - GOT RID OF UNNECESSARY DEBUG DCLS   7/24/86 - tss - removed TOGGLE.INSERT.DEBUG dcl   7/10/86 - TSS - MADE SEQDBUG CODE INTO NORMAL SCREEN   7/08/86 - TSS - ADDED MORE DEBUG DCLS   6/13/86 - TSS - Added some debug dcls   5/15/86 - "official" creation of release-M modules*/DCL SEQDBUG                     FIXED ARRAY EXTERNAL;DCL INIT.SEQ.DEBUG.DISPLAY      PROC EXTERNAL;DCL HANDLE.SEQ.DEBUG.CHAR       PROC (FIXED) EXTERNAL;DCL POLL.FOR.DEBUG.COMMANDS     PROC (FIXED ARRAY) EXTERNAL;DCL DBUG.SCREEN                 PROC(FIXED) EXTERNAL;library ':syntlibs:dbuglib';