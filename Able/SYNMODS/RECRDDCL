/* RECRDDCL -- Memory Block Management Routines */dcl MNullPtr        fixed array external; /* null pointer */dcl R$StkBase       fixed external; /* base of stack in external memory */dcl R$StkMax        fixed external; /* max no. items on stack */dcl PtrToWords      proc(fixed array,fixed array) external; /* turn pointer address into a word address */dcl WordsToPtr      proc(fixed array,fixed array) external; /* turn word address into a pointer address */dcl ReadMBlock      proc(fixed array,fixed) external; /* setup to read block of memory */dcl WriteMBlock     proc(fixed array,fixed) external; /* setup to write block of memory */dcl ReadMPtrBlock   proc(fixed array,fixed,fixed) external; /* setup to read memory block given by pointer */dcl WriteMPtrBlock  proc(fixed array,fixed,fixed) external; /* setup to write memory block given by pointer */dcl CopyMBlock      proc(fixed array,fixed,fixed array,fixed,fixed) external; /* copy block of memory */dcl GetMFree        proc(fixed array) external; /* return no. words free in buffer area */dcl GetMBlockSize   proc(fixed array) returns (fixed) external; /* return no. words used by memory block */dcl NewMBlock       proc(fixed array,fixed,boolean) returns (boolean) external; /* allocate new memory block */dcl DisposeMBlock   proc(fixed array) returns (boolean) external; /* remove memory block */dcl CompareBlock    proc(fixed array,fixed,fixed array) returns (fixed) external; /* compare memory block string to given string */dcl TreeInit        proc(fixed array) returns (boolean) external; /* intialize tree */dcl TreeSearch      proc(fixed array,fixed array,fixed array) returns (boolean) external; /* search tree for string */dcl TreeInsert      proc(fixed array,fixed array,fixed array) returns (fixed) external; /* insert string into tree */dcl TreeTraverse    proc(fixed array,boolean,fixed array) returns (boolean) external; /* traverse tree */dcl DefineMArea     proc(fixed,fixed,fixed,fixed,fixed) external; /* define memory area */dcl DefineStack     proc(fixed,fixed) external; /* define stack area */dcl StackInit       proc external; /* intialize stack */insert  ':synauxs:recrdlit';