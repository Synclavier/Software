/* PARMDCL -- ROUTINES AND DATA FOR SYNCLAVIER KNOB PARAMETERS */DCL (RESET.BITS)      FIXED EXTERNAL;  /* bits are set in this word when knob parameters are changed to cause synthesizer updates to occur */DCL (RESET.XPOS.TIME) FIXED EXTERNAL;DCL PAR.LIMITS   PROC(FIXED) EXTERNAL; /* upper limit of knob entry value */DCL PAR.POINTS   PROC(FIXED) EXTERNAL; /* decimal point look up table     */DCL PAR.UNS      PROC(FIXED) EXTERNAL; /* units look up table             */DCL PAR.TYPE     PROC(FIXED) EXTERNAL; /* display type look up table      */DCL PAR.RESET    PROC(FIXED) EXTERNAL; /* double press (reset) info       */DCL PAR.CHANGE   PROC(FIXED) EXTERNAL; /* bits for reset.bits             */DCL E.SEG.LIMITS DATA  EXTERNAL; /* LIMITS FOR 12 TIMBRE FRAME PARAMETERS */DCL E.SEG.POINTS DATA  EXTERNAL; /* DECIMAL POINTS FOR T. FRAME PARAMETERS */DCL E.SEG.UNS    DATA  EXTERNAL; /* UNITS LED FOR T.FRAME PARAMETERS */LIBRARY ':SYNLIBS:PARMLIB';