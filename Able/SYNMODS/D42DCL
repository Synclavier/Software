/* D42DCL - primitive io routines for d42 */dcl enable_d42      proc external;     /* enables d42 interrupts */dcl disable_d42     proc external;     /* disables d42 interrupts */dcl d42_wait        proc external;     /* wait for printer to finish printing */dcl d42pc           proc(fixed) external;dcl d42.output.happening fixed external; /* cleared when output buffer becomes empty */library ':synlibs:d42lib';