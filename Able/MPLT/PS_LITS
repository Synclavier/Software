/* PS_LITS -- Include file for using the PSPACK D40Q I/O package *//*AUTHOR: Larry Gallagher, New England Digital Co.DESCRIPTION:This file contains the data and procedure declarations for usingthe PSPACK I/O package from an XPL program.USAGE:To use PSPACK, simply 'insert' PS_INCL in your main XPL routine.MODIFICATION HISTORY:   Date      Who      What   ----      ---      ----   6/26/87   LPG      Initial Development   7/6/87    LPG      Added Timed Read, D42 handlers, baud handlers   7/7/87    LPG      Added Reset, generalized Enable/Disable, Timed Write   7/8/87    LPG      Added D40_idle, peek_d40, flush_io   ------    ---      --------------------------------------   7/8/87    LPG      Modified and extended IOPACK to PSPACK   7/9/87    LPG      Added D40_IDLE_MSG   7/10/87   LPG      removed several procs to clean up*/   dcl $Clock fixed external;   dcl $rm_clock fixed external;   dcl $wclock fixed external;   dcl ps$catch_errors boolean external;   dcl ps$error fixed external;   dcl ps$status fixed external;   dcl ps$device fixed external;   dcl ps$msg_text fixed array external;   dcl PS_message lit 'fixed';   dcl printer lit '"40"';   dcl modem   lit '"42"';   dcl ps#null        lit '0';   dcl ps#okay        lit '1';   dcl ps#invalid_dev lit '2';   dcl ps#idle        lit '3';   dcl ps#print_error lit '4';   dcl ps#bad_baud    lit '5';   dcl ps#no_response lit '6';   dcl ps#busy        lit '7';   dcl ps#ptp         lit '8';   dcl ps#init        lit '9';   dcl ps#printing    lit '10';   dcl ps#waiting     lit '11';   dcl ps#exitserver  lit '12';   dcl ps#stackoverflow  lit '31';   dcl ps#invalidaccess  lit '32';   dcl ps#rangecheck     lit '33';   dcl ps#stackunderflow lit '34';   dcl ps#typecheck      lit '35';     dcl ps_wchar lit 'call #ps_wchar';   dcl ps_pstring lit 'call #ps_pstring';   dcl ps_pstringr lit 'call #ps_pstringr';   dcl ps_pnum lit 'call #ps_pnum';   dcl ps_get_string lit 'call #ps_get_string';   dcl PS_ENABLE proc(fixed) external;      dcl PS_DISABLE proc external;   dcl PS_ID_D40Q proc returns (boolean) external;   dcl PS_FLUSH proc external;   dcl PS_SET_BAUD proc(fixed,fixed) external;   dcl PS_IDLE proc returns (boolean) external;   dcl PS_MIDLE proc returns (boolean) external;   dcl #PS_WCHAR proc(fixed) returns (boolean) external;   dcl PS_RCHAR proc returns (fixed) external;   dcl #ps_get_string proc(fixed array) external;   dcl ps_get_num proc returns (fixed) external;   dcl PS_RMCHAR proc returns (fixed) external;   dcl #ps_pstring proc (fixed array) external;   dcl #ps_pstringr proc (fixed array) external;   dcl #ps_pnum proc(fixed) external;   dcl PS_QUERY proc external;/*   dcl PS_PROCESS_MSG proc external; */   dcl ps_clear_typeahead proc external;