insert ':-xpl:termutil';insert ':-xpl:plotlib';punits (1023,779);viewport (0,1023,0,779);iwindow (0,1023,0,779);clear;imove_to (0,600);alpha_mode;pstringr('This is normal alpha mode text.');wchar(27); pstringr('6This is italic alpha mode text!');wchar(27); pstringr('7This is normal alpha mode text.'); crlf;wchar(27); wchar(58);pstringr('This is normal alpha mode text.');wchar(27); pstringr('6This is italic alpha mode text!');wchar(27); pstringr('7This is normal alpha mode text.');call rchar;clear; flush_term;