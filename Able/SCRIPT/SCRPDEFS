/* SCRPDEFS  $TITLE  Script Compiler Token Types - Main Symbols */  DCL T.EOF       LIT '00'; /* ZERO INDICATES END OF FILE */DCL T.EOL       LIT '01'; /* INDICATES END OF LINE IF ENABLE.EOL=1 */DCL T.END       LIT '02'; /* 'END' STATEMENT */DCL T.PARTIAL   LIT '03'; /* 'PARTIAL' */DCL T.TIMBRE    LIT '04'; /* 'TIMBRE' */DCL T.MOTIF     LIT '05'; /* 'MOTIF' */DCL T.SFM       LIT '06'; /* 'SFM' */DCL T.UND       LIT '07'; /* UNDEFINED SYMBOL */DCL T.SCONST    LIT '08'; /* STRING CONSTANT - USED IN 'INSERT' */DCL T.STMT      LIT '09'; /* STATEMENT TYPE - DEFINE, NOTELIST, INSERT, ETC. *//* SUBFIELDS: */DCL T.PFIELD    LIT '10'; /* PARTIAL TIMBRE SUB FIELD */DCL T.TFIELD    LIT '11'; /* TIMBRE SUB FIELD */DCL T.VWAVE     LIT '12'; /* VIBRATO WAVE SHAPE IDENTIFIER */DCL T.PSHAPE    LIT '13'; /* PORTAMENTO SHAPE SPECIFIER - LOG OR LIN */DCL T.RTEP      LIT '14'; /* PARTIAL IDENTIFIER FOR REAL TIME EFFECTS */DCL T.RTEFUN    LIT '15'; /* RTE FUNCTION IDENTIFIER - VE ATTACK, ETC. *//* OTHER: */DCL T.COMMA     LIT '16'; /* COMMA */DCL T.KEY       LIT '17'; /* KEY INDICATATION (C, D, EFLAT, ETC.) */DCL T.TIME      LIT '18'; /* INDICATES STARTING TIME SCANNED */DCL T.INSERT    LIT '19'; /* 'INSERT' STATEMENT */DCL T.USING     LIT '20'; /* 'USING' (FOR NOTELIST) */DCL T.STARTL    LIT '21'; /* 'STARTLOOP' */DCL T.ENDL      LIT '22'; /* 'ENDLOOP' */DCL T.XPOS      LIT '23'; /* 'TRANSPOSE' */DCL T.GFROM     LIT '24'; /* 'GLIDEFROM' */DCL T.HERE      LIT '25'; /* 'HERE' */DCL T.MUL       LIT '26'; /* 'MULTIPLIER' */DCL T.SEC       LIT '27'; /* 'SECOND' */DCL T.FRAMES    LIT '28'; /* 'FRAMES' */DCL T.TO        LIT '29'; /* 'TO' */DCL T.BEAT      LIT '30'; /* 'BEAT' */DCL T.RIT       LIT '31'; /* 'RIT' OR 'ACCEL' */DCL T.IN        LIT '32'; /* 'IN' *//* SYMBOLS FOR WESTERN SCALE FORMATS */DCL T.R         LIT '33'; /* 'R':  RHYTHM LINE */DCL T.A         LIT '34'; /* 'A':  ARTICULATION LINE */DCL T.T         LIT '35'; /* 'T':  TIMBRE LINE */DCL T.V         LIT '36'; /* 'V':  VOLUME LINE */DCL T.P         LIT '37'; /* 'P':  INTERNAL ONLY */DCL T.I         LIT '38'; /* 'I':  INVERSION */DCL T.ST        LIT '39'; /* 'ST': STRICT TRANSPOSITION */DCL T.SI        LIT '40'; /* 'SI': STRICT INVERSION */DCL T.PR        LIT '41'; /* 'PR': PITCH RETROGRADE */DCL T.RR        LIT '42'; /* 'RR': RHYTHM RETROGRADE */DCL T.VBIT      LIT '43'; /* VIBRATO BIT DEFINITIONS */DCL T.NUMBUFFS  LIT '44'; /* NUM.BUFFS STATEMENT FOR SFM SEQUENCE */DCL T.TRACK     LIT '45'; /* TRACK IDENTIFIER FOR NOTELIST STATEMENT */DCL T.FFIELD    LIT '46'; /* FRAME FIELD IN PARTIAL DEFINITION */DCL T.PDEF      LIT '50'; /* USER DEFINED PARTIAL TIMBRE */DCL T.TDEF      LIT '51'; /* USER DEFINED COMPLETE TIMBRE */DCL T.MDEF      LIT '52'; /* USER DEFINED MOTIF */DCL T.SFMDEF    LIT '53'; /* USER DEFINED SFM TIMBRE */DCL T.SYNC      LIT '60'; /* SYNC STATEMENT */DCL T.DELAY     LIT '61'; /* DELAY STATEMENT */DCL T.SMOOTH    LIT '62'; /* SMOOTH STATEMENT */DCL T.PINFO     LIT '63'; /* PINFO AREA  */DCL T.TINFO     LIT '64'; /* TINFO AREA  */DCL T.TNAME     LIT '65'; /* TNAME AREA  */DCL T.VKRTE     LIT '66'; /* VK RTE PATCHING */DCL T.EXPR      LIT '67'; /* EXPRESSION INPUT */DCL T.ROUTING   LIT '68'; /* EXPRESSION ROUTING */DCL T.RELEASEM  LIT '69'; /* ReleaseM */DCL T.GRPHOWMANY LIT '70';DCL T.GRPMEMBERS LIT '71';DCL TEMPO.STATEMENT.#    LIT '04';DCL KEYSIG.STATEMENT.#   LIT '10';DCL P.STATEMENT.#        LIT '15';