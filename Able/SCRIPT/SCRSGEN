/* PROGRAM TO CONSTRUCT SCORE2 SYMBOL TABLE *//* 04/24/89 - cj - added "EVENT" token */DCL BOOLEAN LIT 'FIXED';INSERT ':SCRIPT:SCRPDEFS';INSERT ':-XPL:CATRTNS';  /* PROCEDURES TO BUILD TABLE */  DCL CATBUF (1024) FIXED;DCL BUF(4096)     FIXED; /* OUTPUT BUFFER */DCL PTR           FIXED; /* POINTER TO BUFFER */  PC:  PROC(BYT); /* PUT CHARACTER INTO OUTPUT FILE */  DCL BYT FIXED;  CALL PBYTE(BUF,PTR,BYT); /* STORE BYTE */  PTR=PTR+1; /* INCREMENT */END PC;  SYMBOL:  PROCEDURE(NAM,TYP,INF); /* EMIT A SYMBOL TO TABLE */  DCL NAM       ARRAY;  DCL (TYP,INF) FIXED;  DCL I         FIXED;  I=NAM(0);  CALL PC(I); /* EMIT LENGTH */  DO I=0 TO I-1; CALL PC(BYTE(NAM,I)); END; /* EMIT CHARS */  IF ((INF\TYP)&"177400")<>0 /* LARGE */  THEN PRINT 'ERROR - INFO FIELD TOO LARGE';  CALL PC(TYP); /* EMIT TYPE - 8 BIT ONLY */  CALL PC(INF);END SYMBOL;  /* $SUBTITLE  Define SCORE2 Symbols */CALL SYMBOL('EOF',             T.EOF,      0);CALL SYMBOL('END',             T.END,      0);CALL SYMBOL('PARTIAL',         T.PARTIAL,  0);CALL SYMBOL('PAR',             T.PARTIAL,  0);CALL SYMBOL('TIMBRE',          T.TIMBRE,   0);CALL SYMBOL('TIM',             T.TIMBRE,   0);CALL SYMBOL('SFM',             T.SFM,      0);CALL SYMBOL('MOTIF',           T.MOTIF,    0);CALL SYMBOL('MOT',             T.MOTIF,    0);CALL SYMBOL('INSERT',          T.INSERT,   0);CALL SYMBOL('INS',             T.INSERT,   0);CALL SYMBOL('USING',           T.USING,    0);CALL SYMBOL('USI',             T.USING,    0);CALL SYMBOL('HERE',            T.HERE,     0);CALL SYMBOL('HER',             T.HERE,     0);CALL SYMBOL('MULTIPLIER',      T.MUL,      0);CALL SYMBOL('MUL',             T.MUL,      0);CALL SYMBOL('SECONDS',         T.SEC,      0);CALL SYMBOL('SECOND',          T.SEC,      0);CALL SYMBOL('SEC',             T.SEC,      0);CALL SYMBOL('FRAMES',          T.FRAMES,   0);CALL SYMBOL('FRA',             T.FRAMES,   0);CALL SYMBOL('TO',              T.TO,       0)