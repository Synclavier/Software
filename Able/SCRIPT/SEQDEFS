/* SEQDEFS  $TITLE  SCRIPT Rel I Sequencer Definitions *//* THIS FILE DEFINES LITERAL DECLARATIONS FOR VARIOUS.  ELEMENTS OF THE SEQUENCER DATA STRUCTURE *//* A SEQUENCE IS A LIST OF NUMBERS OF THE FOLLOWING FORMAT:.    1. 1  SECTOR  - MISC INFO (SCALE, SPEED, CLICK RATE, MEMORY POINTERS, ETC)..    2. 12 SECTORS - TIMBRE INFORMATION FOR 16 TRACKS..    3. NOTE LIST  - WORD PAIRS OR TRIPLETS FOR EACH NOTE. */DCL SEQDATA(512)   FIXED;    /* misc data,  or copy of bas.ptr */DCL SEQ.SCALE      LIT '00'; /* LOCATIONS 0-11 - HOLDS ACTIVE SCALE */DCL SEQ.SPEED      LIT '12'; /* HOLDS SEQUENCER SPEED */DCL SEQ.CLICK      LIT '13'; /* HOLDS SEQUENCER CLICK PERIOD */DCL SEQ.CRM        LIT '14'; /* CLICK RATE MULTIPLIER */DCL SEQ.SMODE      LIT '15'; /* SMPTE MODE            */DCL SEQ.SMBITS     LIT '16'; /* SMPTE START # OF BITS */DCL SEQ.SECFRA     LIT '17'; /* SMPTE SECONDS/FRAMES  */DCL SEQ.HRSMIN     LIT '18'; /* SMPTE HRS/MINS        */DCL SPEED          LIT 'SEQDATA(SEQ.SPEED)';DCL CLICK.RATE     LIT 'SEQDATA(SEQ.CLICK)';DCL CRM            LIT 'SEQDATA(SEQ.CRM)';DCL SEQ.MIDI       LIT '20'; /* 20-31: 12-BIT FIELDS FOR 16 TRKS OF MIDI ROUTING */DCL MAGIC.NUMBER   LIT '32'; /* USED TO DETECT VALID DATA */DCL SEQ.OCTRATIO   LIT '33'; /* STORE ACTIVE OCTAVE RATIO HERE */DCL SEQ.TROUT1     LIT '34'; /* 34-41 TRACK ROUTINGS 0-7 */DCL SEQ.LOOP       LIT '42'; /* INDICATES IF LOOPING */DCL SEQ.G.P.T      LIT '43'; /* INDICATES LOOP GO BACK TIME */DCL SEQ.G.P.T.C    LIT '44'; /* GO BACK TIME CARRIES STORED HERE */DCL KBD.XPOS.DATA  LIT '45'; /* MUST BE ZERO (XPOS DIST FOR KEYBOARD) */DCL SPL.XPOS.DATA  LIT '46'; /* MUST BE ZERO (XPOS DIST FOR SPLIT KEYBOARD */DCL SEQ.XPOS.DATA  LIT '47'; /* XPOS DISTANCES - 16 IN ALL */DCL SEQ.LIVE.CLICK LIT '63'; /* LIVE CLICK TRACK FOR SEQUENCER   *//* POINTERS FOR THE DIFFERENT TRACKS ARE STORED HERE: */DCL SEQ.POINTERS   LIT '64'; /* ONE FOR EACH TRACK & FOR TOP */DCL SEQ.TOP        LIT 'SEQDATA(SEQ.POINTERS+16)'; /* POINTER TO TOP */DCL SEQ.NPPW       LIT 'SEQDATA(81)';DCL SEQ.PLEN       LIT '82'; /* LENGTH OF PARAMETER AREA IN NEW FORMAT SEQUENCES */DCL SEQ.SLEN       LIT '80'; /* LENGTH OF SEQUENCER STUFF, INCLUDING 256 MISC WORDS */DCL SEQ.MPREV      LIT '83'; /* MP REV WORD */DCL SEQ.TROUT2     LIT '88'; /* 88-95 TRACK ROUTINGS 8-15 */DCL SEQ.FIRSTS     LIT '96'; /* STORE FIRST NOTES HERE FOR CORRECT PORTAMENTO */DCL SEQ.TRVOLS     LIT '112';/* STORE TRACK VOLUME INFO HERE *//* DATA STORAGE FOR INDEPENDENT LOOPS */DCL SEQ.ILS.PTRS   LIT '128'; /* POINTERS FOR START OF IND LOOPS */DCL SEQ.ILS.PTCS   LIT '144'; /* PLAY TIME CARRIES FOR START OF IND LOOP */DCL SEQ.ILS.PLTS   LIT '160'; /* PLAY TIMES FOR START OF IND LOOP */DCL SEQ.ILE.PTRS   LIT '176'; /* POINTERS FOR END OF IND LOOP */DCL SEQ.ILE.PTCS   LIT '192'; /* PLAY TIME CARRIES FOR END OF IND LOOP */DCL SEQ.ILE.PLTS   LIT '208'; /* PLAY TIMES FOR END OF IND LOOP */DCL SEQ.ILS.LTTS   LIT '224'; /* LAST TRACK TIMES AT LOOP START */DCL SEQ.ILS.NPNS   LIT '240'; /* NUM.PS.NOTES AT LOOP START *//* MAGIC NUMBERS: */DCL MAGIC#1        LIT '"123456"'; /* INDICATES 10 MS. DURATIONS IN SEQ. */DCL MAGIC#2        LIT '"123457"'; /* INDICATES 5  MS. DURATIONS IN SEQ. */DCL MAGIC#3        LIT '"123460"'; /* INDICATES AN SFM SEQUENCE */DCL MAGIC#4        LIT '"123461"'; /* INDICATES SYNCL II SEQUENCE WITH TIMBRE FRAME TIMBRES */DCL MAGIC#5        LIT '"123462"'; /* INDICATES 32 TRACK FORMAT          */DCL MAGIC#6        LIT '"123463"'; /* INDICATES INCLUDE TEMPO MAP        */DCL MAGIC#7        LIT '"123464"'; /* INDICATES INCLUDES GROUPED TRACKS  *//* SPECIAL NOTE TYPES */DCL FIRST.NOTE     LIT  '61'; /* INDICATES START OF TRACK DATA */DCL REST.MARK      LIT '127'; /* INTERNAL TO COMPILER */