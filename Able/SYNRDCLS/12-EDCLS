/* $TITLE 12-EDCLS EXTERNAL DECLARATIONS FOR SEQUENCER EDIT FUNCTIONS *//*   08/27/90 - PF - Support all MIDI controllers   11/05/87 - EG - ADDED TRKS ARGUMENT TO UNWRAP.SEQUENCE   09/04/87 - EG - ADDED PUNCH.REGION   08/28/87 - EG - added paste.silence   04/30/87 - TS - ADDED CUT & PASTE DCLS   11/13/86 - TS - ADDED CHAIN.TRACKS DCL   11/05/86 - TS - ADDED GARBAGE.COLLECT.TRK DCL   11/04/86 - TS - ADDED SOME DCLS HERE*//* FROM EDITPRIM  */DCL GARBAGE.COLLECT.TRK  PROC (FIXED)                         EXTERNAL; /* CLEANS UP AN ENTIRE TRACK */DCL CHECK.INSERT.TIME    PROC (FIXED,FIXED)                   EXTERNAL;DCL CHECK.DELETE.TIME    PROC (FIXED,FIXED,FIXED,FIXED)       EXTERNAL;DCL INSERT.TIME.AT.POINT PROC (FIXED,FIXED,FIXED,FIXED)       EXTERNAL;DCL INSERT.TIME.ON.TRK   PROC (FIXED,FIXED,FIXED,FIXED,FIXED) EXTERNAL;DCL DELETE.TIME.AT.POINT PROC (FIXED,FIXED,FIXED,FIXED)       EXTERNAL;DCL DELETE.TIME.FROM.TRK PROC (FIXED,FIXED,FIXED,FIXED,FIXED) EXTERNAL;DCL FAST.COPY               		PROC (FIXED, FIXED) EXTERNAL;DCL BOUNCE.DOWN              		PROC (FIXED,FIXED,FIXED,FIXED) RETURNS(FIXED) EXTERNAL; /* SPECIFY SOURCE TRACK, DESTINATION TRACK, QUICK FLAG, REPLICATE FLAG */DCL ERASE.TRACK              		PROC (FIXED)        EXTERNAL; /* ERASE ONE TRACK   */DCL ERASE.TRACK.BUT.PRESERVE.GROUP  PROC (FIXED)        EXTERNAL; /* ERASE ONE TRACK; PRESERVE GROUP INFO */DCL DELETE.NOTELIST.FROM.TRK 		PROC (FIXED)        EXTERNAL; /* QUICKLY DELETES AN ENTIRE NOTELIST FROM TRACK */DCL CHAIN.TRACKS          PROC (FIXED,FIXED)                   EXTERNAL;DCL UNWRAP.SEQUENCE       PROC (ARRAY,FIXED,FIXED)    RETURNS(FIXED) EXTERNAL;DCL EDIT.ERROR            FIXED   EXTERNAL; /* GLOBAL ERROR FLAG */DCL APPLY.FILTER          BOOLEAN EXTERNAL; /* true if edit filter should be applied  */dcl check.pitches         fixed external;   dcl check.vel             fixed external;   dcl check.dur             fixed external;   dcl check.ctrl            fixed external;dcl check.monopolar.val	  fixed external;dcl check.bipolar.val	  fixed external;dcl check.midi.val		  fixed external;dcl (ctrl1,			ctrl2,		ctrl3)		fixed external;	/* 3 rte controllers to filter	*/dcl (midi.ctrl1,	midi.ctrl2,	midi.ctrl3)	fixed external;	/* 3 midi controllers to filter	*/dcl (pitch.lo,		pitch.hi)		fixed external;    /* pitch boundaries              */dcl (vel.lo,		vel.hi)			fixed external;    /* velocity boundaries           */dcl (monopolar.lo,monopolar.hi)	fixed external;    /* monopolar RTE value boundaries   */dcl (bipolar.lo,	bipolar.hi)		fixed external;    /* bipolar RTE  value boundaries   */dcl (midi.lo,		midi.hi)			fixed external;    /* midi controller value boundaries   */dcl (dur.lo,		dur.hi)			array external;    /* duration boundaries (32bit)  */DCL CHECK.NOTE.FILTER     PROC EXTERNAL;DCL PUNCH.REGION          PROC (ARRAY,ARRAY,ARRAY,FIXED)       EXTERNAL;DCL PASTE.SILENCE         PROC (ARRAY,ARRAY,ARRAY)             EXTERNAL;DCL PASTE.MERGE.FILL      PROC (FIXED,ARRAY,ARRAY,ARRAY,FIXED) EXTERNAL;DCL CUT.COPY.MOVE         PROC (FIXED,ARRAY,ARRAY,ARRAY,ARRAY) EXTERNAL;