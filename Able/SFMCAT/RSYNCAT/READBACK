dcl (i,j,k)      fixed;dcl buflen       lit '(10*256)';dcl buf (buflen) fixed;dcl start        fixed;dcl len          fixed;print 'Enter starting sector of timbre data file',;input start;call readdata(0,start,buf,256);print 'Compatability:',buf(0);print 'Timbre Length:',buf(1);print 'Frames:       ',buf(2);print 'Coefs/Frame:  ',buf(3);print 'Error Status: ',;do i=0 to buf(2)-1;   if (i&7)=0 then do; print; print '     ',; end;   print buf(4+i),;end;if (buf(2) mod 8)<>0 then print;print 'Timbre Name:  ',string(location(addr(buf(4+buf(2)))));print;len=buf(1);if len IGT buflen then do;   len=buflen;   print 'Necessary to limit length of timbre.';   print;end;call readdata(0,start+1,buf,len);k=0;do i=0 to len-1;   if (i mod 8)=0 then do; print; print k,':  ',; k=k+8; end;   print buf(i),'  ',;end;