/* Tape I/O Library insert file */library ':-xpl:libs:tapelib';insert ':-xpl:tapeface';dcl k#enable_retries  proc external; /* enable tape retries on errors */dcl k#disable_retries proc external; /* disable tape retries on errors */dcl k#read_status     proc external; /* read the tape drive status (and store in TAPE_STATUS) */dcl k#decode_status   proc returns(fixed) external; /* decode the tape drive status */dcl k#load            proc external; /* load the tape */dcl k#unload          proc external; /* unload the tape */dcl k#rewind          proc external; /* rewind the tape */dcl k#select          proc(fixed, boolean) external; /* select a track */dcl k#forward_space   proc external; /* forward space record */dcl k#reverse_space   proc external; /* reverse space record */dcl k#forward_file    proc external; /* forward space file */dcl k#reverse_file    proc external; /* reverse space file */dcl k#erase           proc external; /* erase to LEOT */dcl k#read_tape       proc(fixed array, fixed) returns(fixed) external; /* read data from tape */dcl k#write_tape      proc(fixed array, fixed) returns(fixed) external; /* write data to tape */dcl k#write_tapex     proc(fixed array, fixed) returns(fixed) external; /* write data to tape in extended mode (w/no error recovery) */dcl k#write_filemark  proc(boolean) external; /* write a filemark to tape (extended or otherwise) */