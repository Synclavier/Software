/* Tape interface library insert file */library ':-xpl:libs:tapeface';insert ':-xpl:tapelits';dcl send_command     proc(fixed) returns(boolean) external; /* send a command to the tape drive */dcl send_argument    proc(fixed) returns(boolean) external; /* send a command argument to the tape drive */dcl receive_argument proc returns(fixed) external; /* receive a command result from the tape drive */dcl send_record      proc(fixed array, fixed) returns(fixed) external; /* send a data record to the tape drive */dcl receive_record   proc(fixed array, fixed) returns(fixed) external; /* receive a data record from the tape drive */dcl reset_tape       proc returns(boolean) external; /* reset the tape drive */