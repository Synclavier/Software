/* Define the ASCII character set (and some special VT100 characters) *//* control characters */dcl c.a    lit '"001"'; /* ctrl-a */dcl c.b    lit '"002"'; /* ctrl-b */dcl c.c    lit '"003"'; /* ctrl-c */dcl c.d    lit '"004"'; /* ctrl-d */dcl c.e    lit '"005"'; /* ctrl-e */dcl c.f    lit '"006"'; /* ctrl-f */dcl c.g    lit '"007"'; /* ctrl-g */dcl c.h    lit '"010"'; /* ctrl-h */dcl c.i    lit '"011"'; /* ctrl-i */dcl c.j    lit '"012"'; /* ctrl-j */dcl c.k    lit '"013"'; /* ctrl-k */dcl c.l    lit '"014"'; /* ctrl-l */dcl c.m    lit '"015"'; /* ctrl-m */dcl c.n    lit '"016"'; /* ctrl-n */dcl c.o    lit '"017"'; /* ctrl-o */dcl c.p    lit '"020"'; /* ctrl-p */dcl c.q    lit '"021"'; /* ctrl-q */dcl c.r    lit '"022"'; /* ctrl-r */dcl c.s    lit '"023"'; /* ctrl-s */dcl c.t    lit '"024"'; /* ctrl-t */dcl c.u    lit '"025"'; /* ctrl-u */dcl c.v    lit '"026"'; /* ctrl-v */dcl c.w    lit '"027"'; /* ctrl-w */dcl c.x    lit '"030"'; /* ctrl-x */dcl c.y    lit '"031"'; /* ctrl-y */dcl c.z    lit '"032"'; /* ctrl-z */dcl a.nul  lit '"000"'; /* null */dcl a.null lit '"000"'; /* null */dcl a.soh  lit '"001"'; /* soh  */dcl a.stx  lit '"002"'; /* stx  */dcl a.etx  lit '"003"'; /* etx  */dcl a.eot  lit '"004"'; /* eot  */dcl a.enq  lit '"005"'; /* enq  */dcl a.ack  lit '"006"'; /* ack  */dcl a.bel  lit '"007"'; /* bell */dcl a.bs   lit '"010"'; /* backspace */dcl a.ht   lit '"011"'; /* tab       */dcl a.lf   lit '"012"'; /* linefeed  */dcl a.vt   lit '"013"'; /* vert. tab */dcl a.ff   lit '"014"'; /* formfeed  */dcl a.cr   lit '"015"'; /* return    */dcl a.so   lit '"016"'; /* so   */dcl a.si   lit '"017"'; /* si   */dcl a.dle  lit '"020"'; /* dle  */dcl a.dc1  lit '"021"'; /* dc1  */dcl a.xon  lit '"021"'; /* xon  */dcl a.dc2  lit '"022"'; /* dc2  */dcl a.dc3  lit '"023"'; /* dc3  */dcl a.xoff lit '"023"'; /* xoff */dcl a.dc4  lit '"024"'; /* dc4  */dcl a.nak  lit '"025"'; /* nak  */dcl a.syn  lit '"026"'; /* syn 