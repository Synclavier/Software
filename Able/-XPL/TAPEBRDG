/* Tape bridge insert file */library ':-xpl:libs:tapebrdg';insert ':-xpl:tapelits';dcl decode_status  proc returns (fixed) external;dcl load           proc(fixed) external;dcl unload         proc(fixed, boolean) external;dcl rewind         proc(fixed) external;dcl erase          proc(fixed, boolean) external;dcl select         proc(fixed, fixed) external;dcl forward_space  proc(fixed, fixed, fixed) external;dcl reverse_space  proc(fixed, fixed, fixed) external;dcl forward_file   proc(fixed, fixed, fixed) external;dcl reverse_file   proc(fixed, fixed, fixed) external;dcl read_tape      proc(fixed, fixed array, fixed) external;dcl write_tape     proc(fixed, fixed array, fixed) external;dcl write_filemark proc(fixed, boolean) external;dcl tension_tape   proc(fixed) external;dcl abort_tape     proc(fixed array) external;dcl check_tape     proc external;dcl tape_device    fixed external; /* tape device associated with last operation */dcl tape_position  fixed array external; /* tape position (track and file position) */dcl tape_operation fixed external; /* global operation variable */dcl tape_sense     fixed array external; /* global array to hold extended sense bytes */dcl tape_status    fixed external; /* global tape status variable */