configuration modelb;insert ':-xpl:literals';insert ':-xpl:catrtns';when d03int then do; /* must be a DO so CONHIST can access PCHIST variables */   insert ':-xpl:timer:pchist';end;insert ':-xpl:timer:conhist';when break then call exit (0); /* get out */dcl a lit 'i = j';dcl b lit 'a;a;a;a;a;a;a;a';dcl c lit 'b;b;b;b;b;b;b;b';dcl d lit 'c;c;c;c;c;c;c;c';dcl buf (c#dir_max) fixed; /* catalog buffer */dcl (i, j)          fixed;call set_catbuf (addr(buf (0)), 0);write("3") = 0; write("3") = 0; /* sync up with clock */enable;do while 1;   d;end;