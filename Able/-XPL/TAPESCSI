library ':-xpl:libs:tapescsi';           /* tape code *//* Tape Routines */dcl s#decode_status  proc returns (fixed) external;dcl s#load           proc(fixed) external;dcl s#unload         proc(fixed) external;dcl s#rewind         proc(fixed) external;dcl s#select         proc(fixed, fixed) external;dcl s#forward_space  proc(fixed, fixed, fixed) external;dcl s#reverse_space  proc(fixed, fixed, fixed) external;dcl s#forward_file   proc(fixed, fixed, fixed) external;dcl s#reverse_file   proc(fixed, fixed, fixed) external;dcl s#erase          proc(fixed, fixed) external;dcl s#write_filemark proc(fixed, fixed) external;dcl s#read_tape      proc(fixed, fixed array, fixed) external;dcl s#write_tape     proc(fixed, fixed array, fixed) external;dcl s#copy           proc(fixed, fixed, fixed, fixed, fixed, fixed, fixed) returns (boolean) external;/* Global Status Variables */dcl tape_operation fixed external;       /* global operation variable */dcl tape_sense     fixed array external; /* global array to hold extended sense bytes */dcl tape_status    fixed external;       /* global tape status variable */