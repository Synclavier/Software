/* Able Interpreter Acceptance Test */Configuration modeld, muldiv;print 'Able Interpreter Acceptance Test';print;/* Test carry bit */dcl (i,j,k) fixed;dcl (msb,lsb) fixed;dcl (m,l) fixed;dcl a(200) fixed;m   = 2; l   = 32768;msb = 2; lsb = 32769;m = m + msb;l = l + lsb;if (l ILT lsb)	m = m + 1;	if (m != 5 || l != 1)	print 'Error: Error with 32-bit addition';	/* Test basic comparisons */i = 0; j = 1; k = 32768; l = 65535;if (j < i  or k ILT i  or l ILT i)or (l > i)or (i == j or i == k or i == l)or (i > j  or i IGT k  or i IGT l)	print 'Error: Bad comparison';	/* Test multiply divide	*/i = 0; j = 65535;if  ((i*j != 0)or   (i/j != 0))	print 'Error: Problem with multiply/divide #1';write(5) = 16384; write(4) = 99; write(6) = 16384;if (read(4) != 4096 or read(5) != 99)	print 'Error: Problem with multiply/divide #2';write(7) = 16384;if (read(4) != 99 or read(5) != 16384)	print 'Error: Problem with multiply/divide #3';/* Test floating point (exercises add with carry...) */dcl (f,g,h) floating;f = 1.0000; g = 3.000;h = (f / g) - .3333333;if (abs(h) >= .0000001)	print 'Error: failed floating point #1 (',h,')';if (f/g >= .3333334)	print 'Error: failed floating point #2 (',h,')';if (f/g <= .3333333)	print 'Error: failed floating point #3 (',h,')';	/* Test external memory	*/do i = 0 to 3*2048-1;	write("61") = 99;	write("60") = i;		if ((read("60") != i)	||  (read("61") != 0))		print 'Error: failed external memory #1', read("60"), read("61"), i, 0;end;do i = 3*2048 to 10*2048-1;	write("61") = 99;	write("60") = i;		if ((read("60") != 0)	||  (read("61") != 0))		print 'Error: failed external memory #2', read("60"), read("61"), i, 0;end;do i = 0 to 1000;	write("60") = i;	write("61") = i;		if ((read("60") != i)	||  (read("61") != (i & 0xFF)))		print 'Error: failed external memory #3', read("60"), read("61"), i, i & 0xFF;end;		write("60") = 0;do i = 0 to 1000;	j = read("62");		if ((read("60") != 0)	||  (read("61") != 0))		print 'Error: failed external memory #4', read("60"), read("61"), 0, 0;end;		write("60") = 0;do i = 0 to 1000;	j = read("63");		if ((read("60") != shr(i+1,8))	||  (read("61") != ((i+1) & 0xFF)))		print 'Error: failed external memory #5', read("60"), read("61"), shr(i,8), i & 0xFF;end;		write("60") = 0;do i = 0 to 1000;	write("62") = i;		if ((read("60") != 0)	||  (read("61") != 0))		print 'Error: failed external memory #6', read("60"), read("61"), 0, 0;end;		write("60") = 0;do i = 0 to 1000;	write("63") = i;		if ((read("60") != shr(i+1,8))	||  (read("61") != ((i+1) & 0xFF)))		print 'Error: failed external memory #7', read("60"), read("61"), shr(i,8), i & 0xFF;end;		write("60") = 0;do i = 0 to 1000;	j = read("63");		if ((read("60") != shr(i+1,8))	||  (read("61") != ((i+1) & 0xFF))	||  (i != j))		print 'Error: failed external memory #8', read("60"), read("61"), shr(i,8), i & 0xFF, i, j;end;	/* Test repeat counter memory	*/do i = 0 to 100;	a(i) = i;end;write("313") = addr a(10);write("10")  = -1+3;write("373") = 0;do i = 0 to 9;	if (a(i) != i)		print 'Error: failed repeat counter #1', i, a(i);end;	do i = 10 to 12;	if (a(i) != 0)		print 'Error: failed repeat counter #2', i, a(i);end;	do i = 13 to 100;	if (a(i) != i)		print 'Error: failed repeat counter #3', i, a(i);end;	print 'Completed.';